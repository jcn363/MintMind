      },
      "nsis": {
        "installerIcon": "../resources/win32/code.ico",
        "headerImage": "../resources/win32/inno-big-150.bmp",
        "sidebarImage": "../resources/win32/inno-small-150.bmp",
        "installMode": "perUser"
      }
    }
  }
}