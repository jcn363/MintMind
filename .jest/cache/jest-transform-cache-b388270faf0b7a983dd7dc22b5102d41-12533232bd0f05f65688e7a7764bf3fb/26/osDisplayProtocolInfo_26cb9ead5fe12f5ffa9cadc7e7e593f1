export async function getDisplayProtocol(errorLogger) {
  /* istanbul ignore next */
  cov_1kkbmk7la3().f[0]++;
  const xdgSessionType =
  /* istanbul ignore next */
  (cov_1kkbmk7la3().s[3]++, env[XDG_SESSION_TYPE]);
