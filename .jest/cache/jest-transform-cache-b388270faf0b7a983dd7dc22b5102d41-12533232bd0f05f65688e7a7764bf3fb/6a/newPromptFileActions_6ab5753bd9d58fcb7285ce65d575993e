function getDefaultContentSnippet(promptType, chatModeService) {
  /* istanbul ignore next */
  cov_4xyw5t6fs().f[5]++;
  const agents =
  /* istanbul ignore next */
  (cov_4xyw5t6fs().s[34]++, chatModeService.getModes());
  const agentNames =
  /* istanbul ignore next */
  (cov_4xyw5t6fs().s[35]++, agents.builtin.map(agent => {
    /* istanbul ignore next */
    cov_4xyw5t6fs().f[6]++;
    cov_4xyw5t6fs().s[36]++;
    return agent.name.get();
  }).join(',') + (agents.custom.length ?
  /* istanbul ignore next */
  (cov_4xyw5t6fs().b[7][0]++, ',' + agents.custom.map(agent => {
    /* istanbul ignore next */
    cov_4xyw5t6fs().f[7]++;
    cov_4xyw5t6fs().s[37]++;
    return agent.name.get();
  }).join(',')) :
  /* istanbul ignore next */
  (cov_4xyw5t6fs().b[7][1]++, '')));
  /* istanbul ignore next */
  cov_4xyw5t6fs().s[38]++;
  switch (promptType) {
    case PromptsType.prompt:
      /* istanbul ignore next */
      cov_4xyw5t6fs().b[8][0]++;
      cov_4xyw5t6fs().s[39]++;
      return [`---`, `agent: \${1|${agentNames}|}`, `---`, `\${2:Define the task to achieve, including specific requirements, constraints, and success criteria.}`].join('\n');
    case PromptsType.instructions:
      /* istanbul ignore next */
      cov_4xyw5t6fs().b[8][1]++;
      cov_4xyw5t6fs().s[40]++;
      return [`---`, `applyTo: '\${1|**,**/*.ts|}'`, `---`, `\${2:Provide project context and coding guidelines that AI should follow when generating code, answering questions, or reviewing changes.}`].join('\n');
    case PromptsType.agent:
      /* istanbul ignore next */
      cov_4xyw5t6fs().b[8][2]++;
      cov_4xyw5t6fs().s[41]++;
      return [`---`, `description: '\${1:Describe what this custom agent does and when to use it.}'`, `tools: []`, `---`, `\${2:Define what this custom agent accomplishes for the user, when to use it, and the edges it won't cross. Specify its ideal inputs/outputs, the tools it may call, and how it reports progress or asks for help.}`].join('\n');
