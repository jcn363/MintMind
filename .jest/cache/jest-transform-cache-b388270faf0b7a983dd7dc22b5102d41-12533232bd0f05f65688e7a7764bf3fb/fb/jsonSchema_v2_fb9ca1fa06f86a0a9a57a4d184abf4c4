export function updateTaskDefinitions() {
  /* istanbul ignore next */
  cov_1rgcg4w41p().f[5]++;
  cov_1rgcg4w41p().s[38]++;
  for (const taskType of TaskDefinitionRegistry.all()) {
    /* istanbul ignore next */
    cov_1rgcg4w41p().s[39]++;
    // Check that we haven't already added this task type
    if (taskDefinitions.find(schema => {
      /* istanbul ignore next */
      cov_1rgcg4w41p().f[6]++;
      cov_1rgcg4w41p().s[40]++;
      return schema.properties?.type?.enum?.find ?
      /* istanbul ignore next */
      (cov_1rgcg4w41p().b[6][0]++, schema.properties?.type.enum.find(element => {
        /* istanbul ignore next */
        cov_1rgcg4w41p().f[7]++;
        cov_1rgcg4w41p().s[41]++;
        return element === taskType.taskType;
      })) :
      /* istanbul ignore next */
      (cov_1rgcg4w41p().b[6][1]++, undefined);
    })) {
      /* istanbul ignore next */
      cov_1rgcg4w41p().b[5][0]++;
      cov_1rgcg4w41p().s[42]++;
      continue;
    } else
    /* istanbul ignore next */
    {
      cov_1rgcg4w41p().b[5][1]++;
    }
    const schema =
    /* istanbul ignore next */
    (cov_1rgcg4w41p().s[43]++, Objects.deepClone(taskConfiguration));
    const schemaProperties =
    /* istanbul ignore next */
    (cov_1rgcg4w41p().s[44]++, schema.properties);
    // Since we do this after the schema is assigned we need to patch the refs.
    /* istanbul ignore next */
    cov_1rgcg4w41p().s[45]++;
    schemaProperties.type = {
      type: 'string',
      description: nls.localize('JsonSchema.customizations.customizes.type', 'The task type to customize'),
      enum: [taskType.taskType]
    };
    /* istanbul ignore next */
    cov_1rgcg4w41p().s[46]++;
    if (taskType.required) {
      /* istanbul ignore next */
      cov_1rgcg4w41p().b[7][0]++;
      cov_1rgcg4w41p().s[47]++;
      schema.required = taskType.required.slice();
    } else {
      /* istanbul ignore next */
      cov_1rgcg4w41p().b[7][1]++;
      cov_1rgcg4w41p().s[48]++;
      schema.required = [];
    }
    // Customized tasks require that the task type be set.
    /* istanbul ignore next */
    cov_1rgcg4w41p().s[49]++;
    schema.required.push('type');
    /* istanbul ignore next */
    cov_1rgcg4w41p().s[50]++;
    if (taskType.properties) {
      /* istanbul ignore next */
      cov_1rgcg4w41p().b[8][0]++;
      cov_1rgcg4w41p().s[51]++;
      for (const key of Object.keys(taskType.properties)) {
        const property =
        /* istanbul ignore next */
        (cov_1rgcg4w41p().s[52]++, taskType.properties[key]);
        /* istanbul ignore next */
        cov_1rgcg4w41p().s[53]++;
        schemaProperties[key] = Objects.deepClone(property);
      }
    } else
    /* istanbul ignore next */
    {
      cov_1rgcg4w41p().b[8][1]++;
    }
    cov_1rgcg4w41p().s[54]++;
    fixReferences(schema);
    /* istanbul ignore next */
    cov_1rgcg4w41p().s[55]++;
    taskDefinitions.push(schema);
  }
}
