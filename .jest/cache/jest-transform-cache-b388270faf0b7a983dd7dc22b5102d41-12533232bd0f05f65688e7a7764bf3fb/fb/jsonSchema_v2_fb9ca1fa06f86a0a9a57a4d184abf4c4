function fixReferences(literal) {
  /* istanbul ignore next */
  cov_1rgcg4w41p().f[0]++;
  cov_1rgcg4w41p().s[0]++;
  if (Array.isArray(literal)) {
    /* istanbul ignore next */
    cov_1rgcg4w41p().b[0][0]++;
    cov_1rgcg4w41p().s[1]++;
    literal.forEach(fixReferences);
  } else {
    /* istanbul ignore next */
    cov_1rgcg4w41p().b[0][1]++;
    cov_1rgcg4w41p().s[2]++;
