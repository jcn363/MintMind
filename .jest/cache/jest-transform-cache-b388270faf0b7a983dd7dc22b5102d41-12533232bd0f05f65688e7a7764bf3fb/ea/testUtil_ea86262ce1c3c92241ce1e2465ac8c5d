import { fail, ok } from 'assert';
import { TextureAtlas } from '../../../../browser/gpu/atlas/textureAtlas.js';
import { isNumber } from '../../../../../base/common/types.js';
import { ensureNonNullable } from '../../../../browser/gpu/gpuUtils.js';
export function assertIsValidGlyph(glyph, atlasOrSource) {
  /* istanbul ignore next */
  cov_2i2ai6y2xr().f[0]++;
  cov_2i2ai6y2xr().s[0]++;
  if (glyph === undefined) {
    /* istanbul ignore next */
    cov_2i2ai6y2xr().b[0][0]++;
    cov_2i2ai6y2xr().s[1]++;
    fail('glyph is undefined');
  } else
  /* istanbul ignore next */
  {
    cov_2i2ai6y2xr().b[0][1]++;
  }
  const pageW =
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().s[2]++, atlasOrSource instanceof TextureAtlas ?
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().b[1][0]++, atlasOrSource.pageSize) :
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().b[1][1]++, atlasOrSource.width));
  const pageH =
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().s[3]++, atlasOrSource instanceof TextureAtlas ?
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().b[2][0]++, atlasOrSource.pageSize) :
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().b[2][1]++, atlasOrSource.width));
  const source =
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().s[4]++, atlasOrSource instanceof TextureAtlas ?
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().b[3][0]++, atlasOrSource.pages[glyph.pageIndex].source) :
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().b[3][1]++, atlasOrSource));
  // (x,y) are valid coordinates
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[5]++;
  ok(isNumber(glyph.x));
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[6]++;
  ok(glyph.x >= 0);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[7]++;
  ok(glyph.x < pageW);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[8]++;
  ok(isNumber(glyph.y));
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[9]++;
  ok(glyph.y >= 0);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[10]++;
  ok(glyph.y < pageH);
  // (w,h) are valid dimensions
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[11]++;
  ok(isNumber(glyph.w));
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[12]++;
  ok(glyph.w > 0);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[13]++;
  ok(glyph.w <= pageW);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[14]++;
  ok(isNumber(glyph.h));
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[15]++;
  ok(glyph.h > 0);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[16]++;
  ok(glyph.h <= pageH);
  // (originOffsetX, originOffsetY) are valid offsets
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[17]++;
  ok(isNumber(glyph.originOffsetX));
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[18]++;
  ok(isNumber(glyph.originOffsetY));
  // (x,y) + (w,h) are within the bounds of the atlas
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[19]++;
  ok(glyph.x + glyph.w <= pageW);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[20]++;
  ok(glyph.y + glyph.h <= pageH);
  // Each of the glyph's outer pixel edges contain at least 1 non-transparent pixel
  const ctx =
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().s[21]++, ensureNonNullable(source.getContext('2d')));
  const edges =
  /* istanbul ignore next */
  (cov_2i2ai6y2xr().s[22]++, [ctx.getImageData(glyph.x, glyph.y, glyph.w, 1).data, ctx.getImageData(glyph.x, glyph.y + glyph.h - 1, glyph.w, 1).data, ctx.getImageData(glyph.x, glyph.y, 1, glyph.h).data, ctx.getImageData(glyph.x + glyph.w - 1, glyph.y, 1, glyph.h).data]);
  /* istanbul ignore next */
  cov_2i2ai6y2xr().s[23]++;
  for (const edge of edges) {
    /* istanbul ignore next */
    cov_2i2ai6y2xr().s[24]++;
    ok(edge.some(color => {
      /* istanbul ignore next */
      cov_2i2ai6y2xr().f[1]++;
      cov_2i2ai6y2xr().s[25]++;
      return (color & 0xFF) !== 0;
    }));
  }
}
