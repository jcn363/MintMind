import { Range } from '../../../../editor/common/core/range.js';
import { TextSearchMatch } from './search.js';
function editorMatchToTextSearchResult(matches, model, previewOptions) {
  /* istanbul ignore next */
  cov_bqloa8owe().f[0]++;
  const firstLine =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[0]++, matches[0].range.startLineNumber);
  const lastLine =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[1]++, matches[matches.length - 1].range.endLineNumber);
  const lineTexts =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[2]++, []);
  /* istanbul ignore next */
  cov_bqloa8owe().s[3]++;
  for (let i =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[4]++, firstLine); i <= lastLine; i++) {
    /* istanbul ignore next */
    cov_bqloa8owe().s[5]++;
    lineTexts.push(model.getLineContent(i));
  }
  /* istanbul ignore next */
  cov_bqloa8owe().s[6]++;
  return new TextSearchMatch(lineTexts.join('\n') + '\n', matches.map(m => {
    /* istanbul ignore next */
    cov_bqloa8owe().f[1]++;
    cov_bqloa8owe().s[7]++;
    return new Range(m.range.startLineNumber - 1, m.range.startColumn - 1, m.range.endLineNumber - 1, m.range.endColumn - 1);
  }), previewOptions);
}
/**
 * Combine a set of FindMatches into a set of TextSearchResults. They should be grouped by matches that start on the same line that the previous match ends on.
 */
export function editorMatchesToTextSearchResults(matches, model, previewOptions) {
  /* istanbul ignore next */
  cov_bqloa8owe().f[2]++;
  let previousEndLine =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[8]++, -1);
  const groupedMatches =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[9]++, []);
  let currentMatches =
  /* istanbul ignore next */
  (cov_bqloa8owe().s[10]++, []);
  /* istanbul ignore next */
  cov_bqloa8owe().s[11]++;
  matches.forEach(match => {
    /* istanbul ignore next */
    cov_bqloa8owe().f[3]++;
    cov_bqloa8owe().s[12]++;
    if (match.range.startLineNumber !== previousEndLine) {
      /* istanbul ignore next */
      cov_bqloa8owe().b[0][0]++;
      cov_bqloa8owe().s[13]++;
      currentMatches = [];
      /* istanbul ignore next */
      cov_bqloa8owe().s[14]++;
      groupedMatches.push(currentMatches);
    } else
    /* istanbul ignore next */
    {
      cov_bqloa8owe().b[0][1]++;
    }
    cov_bqloa8owe().s[15]++;
    currentMatches.push(match);
    /* istanbul ignore next */
    cov_bqloa8owe().s[16]++;
    previousEndLine = match.range.endLineNumber;
  });
  /* istanbul ignore next */
  cov_bqloa8owe().s[17]++;
  return groupedMatches.map(sameLineMatches => {
    /* istanbul ignore next */
    cov_bqloa8owe().f[4]++;
    cov_bqloa8owe().s[18]++;
    return editorMatchToTextSearchResult(sameLineMatches, model, previewOptions);
  });
}
