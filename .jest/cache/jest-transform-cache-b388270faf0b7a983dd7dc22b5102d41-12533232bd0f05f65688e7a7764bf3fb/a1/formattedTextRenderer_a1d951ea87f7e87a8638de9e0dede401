              cov_1jvj8m13ox().f[8]++;
              cov_1jvj8m13ox().s[26]++;
              actionHandler.callback(String(treeNode.index), event);
            }));
            /* istanbul ignore next */
            cov_1jvj8m13ox().s[27]++;
            child = a;
          } else {
            /* istanbul ignore next */
            cov_1jvj8m13ox().b[8][1]++;
            cov_1jvj8m13ox().s[28]++;
            if (treeNode.type === 8 /* FormatType.NewLine */) {
              /* istanbul ignore next */
              cov_1jvj8m13ox().b[10][0]++;
              cov_1jvj8m13ox().s[29]++;
              child = document.createElement('br');
            } else {
              /* istanbul ignore next */
              cov_1jvj8m13ox().b[10][1]++;
              cov_1jvj8m13ox().s[30]++;
              if (treeNode.type === 1 /* FormatType.Root */) {
                /* istanbul ignore next */
                cov_1jvj8m13ox().b[11][0]++;
                cov_1jvj8m13ox().s[31]++;
                child = element;
              } else
              /* istanbul ignore next */
              {
                cov_1jvj8m13ox().b[11][1]++;
              }
            }
          }
        }
      }
    }
  }
  /* istanbul ignore next */
  cov_1jvj8m13ox().s[32]++;
  if (
  /* istanbul ignore next */
  (cov_1jvj8m13ox().b[13][0]++, child) &&
  /* istanbul ignore next */
  (cov_1jvj8m13ox().b[13][1]++, element !== child)) {
    /* istanbul ignore next */
    cov_1jvj8m13ox().b[12][0]++;
    cov_1jvj8m13ox().s[33]++;
    element.appendChild(child);
  } else
  /* istanbul ignore next */
  {
    cov_1jvj8m13ox().b[12][1]++;
  }
  cov_1jvj8m13ox().s[34]++;
  if (
  /* istanbul ignore next */
  (cov_1jvj8m13ox().b[15][0]++, child) &&
  /* istanbul ignore next */
  (cov_1jvj8m13ox().b[15][1]++, Array.isArray(treeNode.children))) {
    /* istanbul ignore next */
    cov_1jvj8m13ox().b[14][0]++;
    cov_1jvj8m13ox().s[35]++;
    treeNode.children.forEach(nodeChild => {
      /* istanbul ignore next */
      cov_1jvj8m13ox().f[9]++;
      cov_1jvj8m13ox().s[36]++;
      _renderFormattedText(child, nodeChild, actionHandler, renderCodeSegments);
    });
  } else
  /* istanbul ignore next */
  {
    cov_1jvj8m13ox().b[14][1]++;
  }
}
function parseFormattedText(content, parseCodeSegments) {
  /* istanbul ignore next */
  cov_1jvj8m13ox().f[10]++;
  const root =
  /* istanbul ignore next */
  (cov_1jvj8m13ox().s[37]++, {
    type: 1 /* FormatType.Root */,
    children: []
  });
  let actionViewItemIndex =
  /* istanbul ignore next */
  (cov_1jvj8m13ox().s[38]++, 0);
  let current =
  /* istanbul ignore next */
  (cov_1jvj8m13ox().s[39]++, root);
  const stack =
  /* istanbul ignore next */
  (cov_1jvj8m13ox().s[40]++, []);
  const stream =
  /* istanbul ignore next */
  (cov_1jvj8m13ox().s[41]++, new StringStream(content));
  /* istanbul ignore next */
  cov_1jvj8m13ox().s[42]++;
  while (!stream.eos()) {
    let next =
    /* istanbul ignore next */
    (cov_1jvj8m13ox().s[43]++, stream.next());
    const isEscapedFormatType =
    /* istanbul ignore next */
    (cov_1jvj8m13ox().s[44]++,
    /* istanbul ignore next */
    (cov_1jvj8m13ox().b[16][0]++, next === '\\') &&
    /* istanbul ignore next */
    (cov_1jvj8m13ox().b[16][1]++, formatTagType(stream.peek(), parseCodeSegments) !== 0) /* FormatType.Invalid */);
    /* istanbul ignore next */
    cov_1jvj8m13ox().s[45]++;
    if (isEscapedFormatType) {
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[17][0]++;
      cov_1jvj8m13ox().s[46]++;
      next = stream.next(); // unread the backslash if it escapes a format tag type
    } else
    /* istanbul ignore next */
    {
      cov_1jvj8m13ox().b[17][1]++;
    }
    cov_1jvj8m13ox().s[47]++;
    if (
    /* istanbul ignore next */
    (cov_1jvj8m13ox().b[19][0]++, !isEscapedFormatType) &&
    /* istanbul ignore next */
    (cov_1jvj8m13ox().b[19][1]++, isFormatTag(next, parseCodeSegments)) &&
    /* istanbul ignore next */
    (cov_1jvj8m13ox().b[19][2]++, next === stream.peek())) {
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[18][0]++;
      cov_1jvj8m13ox().s[48]++;
      stream.advance();
      /* istanbul ignore next */
      cov_1jvj8m13ox().s[49]++;
      if (current.type === 2 /* FormatType.Text */) {
        /* istanbul ignore next */
        cov_1jvj8m13ox().b[20][0]++;
        cov_1jvj8m13ox().s[50]++;
        current = stack.pop();
      } else
      /* istanbul ignore next */
      {
        cov_1jvj8m13ox().b[20][1]++;
      }
      const type =
      /* istanbul ignore next */
      (cov_1jvj8m13ox().s[51]++, formatTagType(next, parseCodeSegments));
      /* istanbul ignore next */
      cov_1jvj8m13ox().s[52]++;
      if (
      /* istanbul ignore next */
      (cov_1jvj8m13ox().b[22][0]++, current.type === type) ||
      /* istanbul ignore next */
      (cov_1jvj8m13ox().b[22][1]++, current.type === 5 /* FormatType.Action */) &&
      /* istanbul ignore next */
      (cov_1jvj8m13ox().b[22][2]++, type === 6) /* FormatType.ActionClose */) {
        /* istanbul ignore next */
        cov_1jvj8m13ox().b[21][0]++;
        cov_1jvj8m13ox().s[53]++;
        current = stack.pop();
      } else {
        /* istanbul ignore next */
        cov_1jvj8m13ox().b[21][1]++;
        const newCurrent =
        /* istanbul ignore next */
        (cov_1jvj8m13ox().s[54]++, {
          type: type,
          children: []
        });
        /* istanbul ignore next */
        cov_1jvj8m13ox().s[55]++;
        if (type === 5 /* FormatType.Action */) {
          /* istanbul ignore next */
          cov_1jvj8m13ox().b[23][0]++;
          cov_1jvj8m13ox().s[56]++;
          newCurrent.index = actionViewItemIndex;
          /* istanbul ignore next */
          cov_1jvj8m13ox().s[57]++;
          actionViewItemIndex++;
        } else
        /* istanbul ignore next */
        {
          cov_1jvj8m13ox().b[23][1]++;
        }
        cov_1jvj8m13ox().s[58]++;
        current.children.push(newCurrent);
        /* istanbul ignore next */
        cov_1jvj8m13ox().s[59]++;
        stack.push(current);
        /* istanbul ignore next */
        cov_1jvj8m13ox().s[60]++;
        current = newCurrent;
      }
    } else {
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[18][1]++;
      cov_1jvj8m13ox().s[61]++;
      if (next === '\n') {
        /* istanbul ignore next */
        cov_1jvj8m13ox().b[24][0]++;
        cov_1jvj8m13ox().s[62]++;
        if (current.type === 2 /* FormatType.Text */) {
          /* istanbul ignore next */
          cov_1jvj8m13ox().b[25][0]++;
          cov_1jvj8m13ox().s[63]++;
          current = stack.pop();
        } else
        /* istanbul ignore next */
        {
          cov_1jvj8m13ox().b[25][1]++;
        }
        cov_1jvj8m13ox().s[64]++;
        current.children.push({
          type: 8 /* FormatType.NewLine */
        });
      } else {
        /* istanbul ignore next */
        cov_1jvj8m13ox().b[24][1]++;
        cov_1jvj8m13ox().s[65]++;
        if (current.type !== 2 /* FormatType.Text */) {
          /* istanbul ignore next */
          cov_1jvj8m13ox().b[26][0]++;
          const textCurrent =
          /* istanbul ignore next */
          (cov_1jvj8m13ox().s[66]++, {
            type: 2 /* FormatType.Text */,
            content: next
          });
          /* istanbul ignore next */
          cov_1jvj8m13ox().s[67]++;
          current.children.push(textCurrent);
          /* istanbul ignore next */
          cov_1jvj8m13ox().s[68]++;
          stack.push(current);
          /* istanbul ignore next */
          cov_1jvj8m13ox().s[69]++;
          current = textCurrent;
        } else {
          /* istanbul ignore next */
          cov_1jvj8m13ox().b[26][1]++;
          cov_1jvj8m13ox().s[70]++;
          current.content += next;
        }
      }
    }
  }
  /* istanbul ignore next */
  cov_1jvj8m13ox().s[71]++;
  if (current.type === 2 /* FormatType.Text */) {
    /* istanbul ignore next */
    cov_1jvj8m13ox().b[27][0]++;
    cov_1jvj8m13ox().s[72]++;
    current = stack.pop();
  } else
  /* istanbul ignore next */
  {
    cov_1jvj8m13ox().b[27][1]++;
  }
  cov_1jvj8m13ox().s[73]++;
  if (stack.length) {
    /* istanbul ignore next */
    cov_1jvj8m13ox().b[28][0]++;
  } // incorrectly formatted string literal
  else
  /* istanbul ignore next */
  {
    cov_1jvj8m13ox().b[28][1]++;
  }
  cov_1jvj8m13ox().s[74]++;
  return root;
}
function isFormatTag(char, supportCodeSegments) {
  /* istanbul ignore next */
  cov_1jvj8m13ox().f[11]++;
  cov_1jvj8m13ox().s[75]++;
  return formatTagType(char, supportCodeSegments) !== 0 /* FormatType.Invalid */;
}
function formatTagType(char, supportCodeSegments) {
  /* istanbul ignore next */
  cov_1jvj8m13ox().f[12]++;
  cov_1jvj8m13ox().s[76]++;
  switch (char) {
    case '*':
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[29][0]++;
      cov_1jvj8m13ox().s[77]++;
      return 3 /* FormatType.Bold */;
    case '_':
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[29][1]++;
      cov_1jvj8m13ox().s[78]++;
      return 4 /* FormatType.Italics */;
    case '[':
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[29][2]++;
      cov_1jvj8m13ox().s[79]++;
      return 5 /* FormatType.Action */;
    case ']':
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[29][3]++;
      cov_1jvj8m13ox().s[80]++;
      return 6 /* FormatType.ActionClose */;
    case '`':
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[29][4]++;
      cov_1jvj8m13ox().s[81]++;
      return supportCodeSegments ?
      /* istanbul ignore next */
      (cov_1jvj8m13ox().b[30][0]++, 7 /* FormatType.Code */) :
      /* istanbul ignore next */
      (cov_1jvj8m13ox().b[30][1]++, 0) /* FormatType.Invalid */;
    default:
      /* istanbul ignore next */
      cov_1jvj8m13ox().b[29][5]++;
      cov_1jvj8m13ox().s[82]++;
      return 0 /* FormatType.Invalid */;
  }
}
