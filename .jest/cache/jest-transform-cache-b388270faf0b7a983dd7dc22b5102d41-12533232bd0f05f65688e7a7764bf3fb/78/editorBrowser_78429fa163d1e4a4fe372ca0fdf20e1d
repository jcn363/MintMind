  cov_24wsf4ey8d().s[11]++;
  if (
  /* istanbul ignore next */
  (cov_24wsf4ey8d().b[8][0]++, isCompositeEditor(thing)) &&
  /* istanbul ignore next */
  (cov_24wsf4ey8d().b[8][1]++, isCodeEditor(thing.activeCodeEditor))) {
    /* istanbul ignore next */
    cov_24wsf4ey8d().b[7][0]++;
    cov_24wsf4ey8d().s[12]++;
    return thing.activeCodeEditor;
  } else
  /* istanbul ignore next */
  {
    cov_24wsf4ey8d().b[7][1]++;
  }
  cov_24wsf4ey8d().s[13]++;
  return null;
}
