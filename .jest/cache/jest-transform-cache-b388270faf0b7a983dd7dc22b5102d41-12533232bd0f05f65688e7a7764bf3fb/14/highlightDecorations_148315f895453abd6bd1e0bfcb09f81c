export function getHighlightDecorationOptions(kind) {
  /* istanbul ignore next */
  cov_1675lt5kx2().f[0]++;
  cov_1675lt5kx2().s[14]++;
  if (kind === DocumentHighlightKind.Write) {
    /* istanbul ignore next */
    cov_1675lt5kx2().b[0][0]++;
    cov_1675lt5kx2().s[15]++;
    return _WRITE_OPTIONS;
  } else {
    /* istanbul ignore next */
    cov_1675lt5kx2().b[0][1]++;
    cov_1675lt5kx2().s[16]++;
    if (kind === DocumentHighlightKind.Text) {
      /* istanbul ignore next */
      cov_1675lt5kx2().b[1][0]++;
      cov_1675lt5kx2().s[17]++;
      return _TEXT_OPTIONS;
    } else {
      /* istanbul ignore next */
      cov_1675lt5kx2().b[1][1]++;
      cov_1675lt5kx2().s[18]++;
      return _REGULAR_OPTIONS;
    }
  }
}
