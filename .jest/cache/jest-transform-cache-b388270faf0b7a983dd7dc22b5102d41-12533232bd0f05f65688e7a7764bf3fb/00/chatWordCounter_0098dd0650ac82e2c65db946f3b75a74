  /* istanbul ignore next */
  (cov_1arpcz4ydv().s[7]++, str.substring(0, endIndex));
  /* istanbul ignore next */
  cov_1arpcz4ydv().s[8]++;
  return {
    value,
    returnedWordCount: targetWords.length === 0 ?
    /* istanbul ignore next */
    (cov_1arpcz4ydv().b[2][0]++, value.length ?
