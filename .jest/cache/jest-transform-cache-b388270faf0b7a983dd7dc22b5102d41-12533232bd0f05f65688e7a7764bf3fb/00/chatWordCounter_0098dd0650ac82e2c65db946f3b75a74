  /* istanbul ignore next */
  (cov_1arpcz4ydv().s[4]++, Array.from(str.matchAll(wordRegExp)));
  const targetWords =
  /* istanbul ignore next */
  (cov_1arpcz4ydv().s[5]++, allWordMatches.slice(0, numWordsToCount));
  const endIndex =
  /* istanbul ignore next */
  (cov_1arpcz4ydv().s[6]++, numWordsToCount >= allWordMatches.length ?
  /* istanbul ignore next */
  (cov_1arpcz4ydv().b[0][0]++, str.length // Reached end of string
