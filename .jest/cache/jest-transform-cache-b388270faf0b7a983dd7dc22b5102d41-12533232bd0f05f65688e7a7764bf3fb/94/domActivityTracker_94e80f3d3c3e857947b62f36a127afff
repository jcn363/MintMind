      /* istanbul ignore next */
      cov_1vcdd1rjuw().f[2]++;
      cov_1vcdd1rjuw().s[13]++;
      // if was inactive, they've now returned
      if (intervalsWithoutActivity === MIN_INTERVALS_WITHOUT_ACTIVITY) {
        /* istanbul ignore next */
        cov_1vcdd1rjuw().b[1][0]++;
        cov_1vcdd1rjuw().s[14]++;
        activeMutex.value = userActivityService.markActive();
