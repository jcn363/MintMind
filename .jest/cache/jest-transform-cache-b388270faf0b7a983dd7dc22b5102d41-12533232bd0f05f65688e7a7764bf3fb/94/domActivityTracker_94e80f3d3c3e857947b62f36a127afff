        cov_1vcdd1rjuw().s[15]++;
        intervalTimer.cancelAndSet(onInterval, CHECK_INTERVAL, targetWindow);
      } else
      /* istanbul ignore next */
      {
        cov_1vcdd1rjuw().b[1][1]++;
      }
      cov_1vcdd1rjuw().s[16]++;
      intervalsWithoutActivity = 0;
