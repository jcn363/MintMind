export function priorityToRank(priority) {
  /* istanbul ignore next */
  cov_1kr60t49gw().f[1]++;
  cov_1kr60t49gw().s[10]++;
  switch (priority) {
    case RegisteredEditorPriority.exclusive:
      /* istanbul ignore next */
      cov_1kr60t49gw().b[1][0]++;
      cov_1kr60t49gw().s[11]++;
      return 5;
    case RegisteredEditorPriority.default:
      /* istanbul ignore next */
      cov_1kr60t49gw().b[1][1]++;
      cov_1kr60t49gw().s[12]++;
      return 4;
    case RegisteredEditorPriority.builtin:
      /* istanbul ignore next */
      cov_1kr60t49gw().b[1][2]++;
      cov_1kr60t49gw().s[13]++;
      return 3;
    // Text editor is priority 2
    case RegisteredEditorPriority.option:
      /* istanbul ignore next */
      cov_1kr60t49gw().b[1][3]++;
    default:
      /* istanbul ignore next */
      cov_1kr60t49gw().b[1][4]++;
      cov_1kr60t49gw().s[14]++;
      return 1;
  }
}
export function globMatchesResource(globPattern, resource) {
  /* istanbul ignore next */
  cov_1kr60t49gw().f[2]++;
  const excludedSchemes =
  /* istanbul ignore next */
  (cov_1kr60t49gw().s[15]++, new Set([Schemas.extension, Schemas.webviewPanel, Schemas.vscodeWorkspaceTrust, Schemas.vscodeSettings]));
  // We want to say that the above schemes match no glob patterns
  /* istanbul ignore next */
  cov_1kr60t49gw().s[16]++;
  if (excludedSchemes.has(resource.scheme)) {
    /* istanbul ignore next */
    cov_1kr60t49gw().b[2][0]++;
    cov_1kr60t49gw().s[17]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_1kr60t49gw().b[2][1]++;
  }
  const matchOnPath =
  /* istanbul ignore next */
  (cov_1kr60t49gw().s[18]++,
  /* istanbul ignore next */
  (cov_1kr60t49gw().b[3][0]++, typeof globPattern === 'string') &&
  /* istanbul ignore next */
  (cov_1kr60t49gw().b[3][1]++, globPattern.indexOf(posix.sep) >= 0));
  const target =
  /* istanbul ignore next */
  (cov_1kr60t49gw().s[19]++, matchOnPath ?
  /* istanbul ignore next */
  (cov_1kr60t49gw().b[4][0]++, `${resource.scheme}:${resource.path}`) :
  /* istanbul ignore next */
  (cov_1kr60t49gw().b[4][1]++, basename(resource)));
  /* istanbul ignore next */
  cov_1kr60t49gw().s[20]++;
  return glob.match(globPattern, target, {
    ignoreCase: true
  });
}
