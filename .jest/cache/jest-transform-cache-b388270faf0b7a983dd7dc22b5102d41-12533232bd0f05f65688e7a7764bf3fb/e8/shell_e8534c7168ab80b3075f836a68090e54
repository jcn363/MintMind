      cov_286kozif2t().s[15]++;
      if (!unixLikeTerminal) {
        /* istanbul ignore next */
        cov_286kozif2t().b[7][0]++;
        cov_286kozif2t().s[16]++;
        unixLikeTerminal = 'sh';
      } else
      /* istanbul ignore next */
      {
        cov_286kozif2t().b[7][1]++;
      }
      // Some systems have $SHELL set to /bin/false which breaks the terminal
      cov_286kozif2t().s[17]++;
      if (unixLikeTerminal === '/bin/false') {
        /* istanbul ignore next */
        cov_286kozif2t().b[8][0]++;
        cov_286kozif2t().s[18]++;
        unixLikeTerminal = '/bin/bash';
      } else
      /* istanbul ignore next */
      {
        cov_286kozif2t().b[8][1]++;
      }
    }
    /* istanbul ignore next */
    cov_286kozif2t().s[19]++;
    _TERMINAL_DEFAULT_SHELL_UNIX_LIKE = unixLikeTerminal;
  } else
  /* istanbul ignore next */
  {
    cov_286kozif2t().b[4][1]++;
  }
  cov_286kozif2t().s[20]++;
  return _TERMINAL_DEFAULT_SHELL_UNIX_LIKE;
}
let _TERMINAL_DEFAULT_SHELL_WINDOWS =
/* istanbul ignore next */
(cov_286kozif2t().s[21]++, null);
async function getSystemShellWindows() {
  /* istanbul ignore next */
  cov_286kozif2t().f[2]++;
  cov_286kozif2t().s[22]++;
  if (!_TERMINAL_DEFAULT_SHELL_WINDOWS) {
    /* istanbul ignore next */
    cov_286kozif2t().b[9][0]++;
    cov_286kozif2t().s[23]++;
    _TERMINAL_DEFAULT_SHELL_WINDOWS = (await getFirstAvailablePowerShellInstallation()).exePath;
  } else
  /* istanbul ignore next */
  {
    cov_286kozif2t().b[9][1]++;
  }
  cov_286kozif2t().s[24]++;
  return _TERMINAL_DEFAULT_SHELL_WINDOWS;
