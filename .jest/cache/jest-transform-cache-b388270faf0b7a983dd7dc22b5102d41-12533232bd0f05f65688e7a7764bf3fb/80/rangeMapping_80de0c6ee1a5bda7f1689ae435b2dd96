export class SingleRangeMapping {
  original;
  modified;
  constructor(original, modified) {
    /* istanbul ignore next */
    cov_gsw4zsf1l().f[6]++;
    cov_gsw4zsf1l().s[14]++;
    this.original = original;
    /* istanbul ignore next */
    cov_gsw4zsf1l().s[15]++;
    this.modified = modified;
  }
  reverse() {
    /* istanbul ignore next */
    cov_gsw4zsf1l().f[7]++;
    cov_gsw4zsf1l().s[16]++;
