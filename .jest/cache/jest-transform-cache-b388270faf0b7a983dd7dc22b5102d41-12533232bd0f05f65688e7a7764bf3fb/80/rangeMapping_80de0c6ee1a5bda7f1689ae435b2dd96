/**
 * Represents a list of mappings of ranges from one document to another.
 */
export class RangeMapping {
  mappings;
  constructor(mappings) {
    /* istanbul ignore next */
    cov_gsw4zsf1l().f[0]++;
    cov_gsw4zsf1l().s[0]++;
