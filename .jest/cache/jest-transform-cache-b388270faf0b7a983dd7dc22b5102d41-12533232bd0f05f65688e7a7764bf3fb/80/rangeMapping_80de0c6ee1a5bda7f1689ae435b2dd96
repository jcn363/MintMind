    /* istanbul ignore next */
    cov_gsw4zsf1l().f[8]++;
    cov_gsw4zsf1l().s[17]++;
    return `${this.original.toString()} -> ${this.modified.toString()}`;
  }
}
export class PositionOrRange {
  position;
