// when doing sampling
const ratio=/* istanbul ignore next */(cov_jdwbs343p().s[302]++,viewLineCount/minimapLineCount);/* istanbul ignore next */cov_jdwbs343p().s[303]++;if(ratio>1){/* istanbul ignore next */cov_jdwbs343p().b[86][0]++;cov_jdwbs343p().s[304]++;minimapHeightIsEditorHeight=true;/* istanbul ignore next */cov_jdwbs343p().s[305]++;minimapIsSampling=true;/* istanbul ignore next */cov_jdwbs343p().s[306]++;minimapScale=1;/* istanbul ignore next */cov_jdwbs343p().s[307]++;minimapLineHeight=1;/* istanbul ignore next */cov_jdwbs343p().s[308]++;minimapCharWidth=minimapScale/pixelRatio;}else{/* istanbul ignore next */cov_jdwbs343p().b[86][1]++;let fitBecomesFill=/* istanbul ignore next */(cov_jdwbs343p().s[309]++,false);let maxMinimapScale=/* istanbul ignore next */(cov_jdwbs343p().s[310]++,minimapScale+1);/* istanbul ignore next */cov_jdwbs343p().s[311]++;if(minimapSize==='fit'){/* istanbul ignore next */cov_jdwbs343p().b[87][0]++;const effectiveMinimapHeight=/* istanbul ignore next */(cov_jdwbs343p().s[312]++,Math.ceil((extraLinesBeforeFirstLine+viewLineCount+extraLinesBeyondLastLine)*minimapLineHeight));/* istanbul ignore next */cov_jdwbs343p().s[313]++;if(/* istanbul ignore next */(cov_jdwbs343p().b[89][0]++,isViewportWrapping)&&/* istanbul ignore next */(cov_jdwbs343p().b[89][1]++,couldUseMemory)&&/* istanbul ignore next */(cov_jdwbs343p().b[89][2]++,remainingWidth<=memory.stableFitRemainingWidth)){/* istanbul ignore next */cov_jdwbs343p().b[88][0]++;cov_jdwbs343p().s[314]++;// There is a loop when using `fit` and viewport wrapping:
// - view line count impacts minimap layout
// - minimap layout impacts viewport width
// - viewport width impacts view line count
// To break the loop, once we go to a smaller minimap scale, we try to stick with it.
fitBecomesFill=true;/* istanbul ignore next */cov_jdwbs343p().s[315]++;maxMinimapScale=memory.stableFitMaxMinimapScale;}else{/* istanbul ignore next */cov_jdwbs343p().b[88][1]++;cov_jdwbs343p().s[316]++;fitBecomesFill=effectiveMinimapHeight>minimapCanvasInnerHeight;}}else/* istanbul ignore next */{cov_jdwbs343p().b[87][1]++;}cov_jdwbs343p().s[317]++;if(/* istanbul ignore next */(cov_jdwbs343p().b[91][0]++,minimapSize==='fill')||/* istanbul ignore next */(cov_jdwbs343p().b[91][1]++,fitBecomesFill)){/* istanbul ignore next */cov_jdwbs343p().b[90][0]++;cov_jdwbs343p().s[318]++;minimapHeightIsEditorHeight=true;const configuredMinimapScale=/* istanbul ignore next */(cov_jdwbs343p().s[319]++,minimapScale);/* istanbul ignore next */cov_jdwbs343p().s[320]++;minimapLineHeight=Math.min(lineHeight*pixelRatio,Math.max(1,Math.floor(1/desiredRatio)));/* istanbul ignore next */cov_jdwbs343p().s[321]++;if(/* istanbul ignore next */(cov_jdwbs343p().b[93][0]++,isViewportWrapping)&&/* istanbul ignore next */(cov_jdwbs343p().b[93][1]++,couldUseMemory)&&/* istanbul ignore next */(cov_jdwbs343p().b[93][2]++,remainingWidth<=memory.stableFitRemainingWidth)){/* istanbul ignore next */cov_jdwbs343p().b[92][0]++;cov_jdwbs343p().s[322]++;// There is a loop when using `fill` and viewport wrapping:
// - view line count impacts minimap layout
// - minimap layout impacts viewport width
// - viewport width impacts view line count
// To break the loop, once we go to a smaller minimap scale, we try to stick with it.
maxMinimapScale=memory.stableFitMaxMinimapScale;}else/* istanbul ignore next */{cov_jdwbs343p().b[92][1]++;}cov_jdwbs343p().s[323]++;minimapScale=Math.min(maxMinimapScale,Math.max(1,Math.floor(minimapLineHeight/baseCharHeight)));/* istanbul ignore next */cov_jdwbs343p().s[324]++;if(minimapScale>configuredMinimapScale){/* istanbul ignore next */cov_jdwbs343p().b[94][0]++;cov_jdwbs343p().s[325]++;minimapWidthMultiplier=Math.min(2,minimapScale/configuredMinimapScale);}else/* istanbul ignore next */{cov_jdwbs343p().b[94][1]++;}cov_jdwbs343p().s[326]++;minimapCharWidth=minimapScale/pixelRatio/minimapWidthMultiplier;/* istanbul ignore next */cov_jdwbs343p().s[327]++;minimapCanvasInnerHeight=Math.ceil(Math.max(typicalViewportLineCount,extraLinesBeforeFirstLine+viewLineCount+extraLinesBeyondLastLine)*minimapLineHeight);/* istanbul ignore next */cov_jdwbs343p().s[328]++;if(isViewportWrapping){/* istanbul ignore next */cov_jdwbs343p().b[95][0]++;cov_jdwbs343p().s[329]++;// remember for next time
