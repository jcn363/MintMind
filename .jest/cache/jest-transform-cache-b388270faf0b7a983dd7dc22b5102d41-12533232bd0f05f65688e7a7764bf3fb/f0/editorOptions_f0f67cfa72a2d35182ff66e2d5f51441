// (leaving 2px for the cursor to have space after the last character)
// viewportColumn = (contentWidth - verticalScrollbarWidth - 2) / typicalHalfwidthCharacterWidth
// minimapWidth = viewportColumn * minimapCharWidth
// contentWidth = remainingWidth - minimapWidth
// What are good values for contentWidth and minimapWidth ?
// minimapWidth = ((contentWidth - verticalScrollbarWidth - 2) / typicalHalfwidthCharacterWidth) * minimapCharWidth
// typicalHalfwidthCharacterWidth * minimapWidth = (contentWidth - verticalScrollbarWidth - 2) * minimapCharWidth
// typicalHalfwidthCharacterWidth * minimapWidth = (remainingWidth - minimapWidth - verticalScrollbarWidth - 2) * minimapCharWidth
// (typicalHalfwidthCharacterWidth + minimapCharWidth) * minimapWidth = (remainingWidth - verticalScrollbarWidth - 2) * minimapCharWidth
// minimapWidth = ((remainingWidth - verticalScrollbarWidth - 2) * minimapCharWidth) / (typicalHalfwidthCharacterWidth + minimapCharWidth)
const minimapMaxWidth=/* istanbul ignore next */(cov_jdwbs343p().s[334]++,Math.floor(minimapMaxColumn*minimapCharWidth));const minimapWidth=/* istanbul ignore next */(cov_jdwbs343p().s[335]++,Math.min(minimapMaxWidth,Math.max(0,Math.floor((remainingWidth-verticalScrollbarWidth-2)*minimapCharWidth/(typicalHalfwidthCharacterWidth+minimapCharWidth)))+MINIMAP_GUTTER_WIDTH));let minimapCanvasInnerWidth=/* istanbul ignore next */(cov_jdwbs343p().s[336]++,Math.floor(pixelRatio*minimapWidth));const minimapCanvasOuterWidth=/* istanbul ignore next */(cov_jdwbs343p().s[337]++,minimapCanvasInnerWidth/pixelRatio);/* istanbul ignore next */cov_jdwbs343p().s[338]++;minimapCanvasInnerWidth=Math.floor(minimapCanvasInnerWidth*minimapWidthMultiplier);const renderMinimap=/* istanbul ignore next */(cov_jdwbs343p().s[339]++,minimapRenderCharacters?/* istanbul ignore next */(cov_jdwbs343p().b[96][0]++,1/* RenderMinimap.Text */):/* istanbul ignore next */(cov_jdwbs343p().b[96][1]++,2)/* RenderMinimap.Blocks */);const minimapLeft=/* istanbul ignore next */(cov_jdwbs343p().s[340]++,minimapSide==='left'?/* istanbul ignore next */(cov_jdwbs343p().b[97][0]++,0):/* istanbul ignore next */(cov_jdwbs343p().b[97][1]++,outerWidth-minimapWidth-verticalScrollbarWidth));/* istanbul ignore next */cov_jdwbs343p().s[341]++;return{renderMinimap,minimapLeft,minimapWidth,minimapHeightIsEditorHeight,minimapIsSampling,minimapScale,minimapLineHeight,minimapCanvasInnerWidth,minimapCanvasInnerHeight,minimapCanvasOuterWidth,minimapCanvasOuterHeight};}static computeLayout(options,env){/* istanbul ignore next */cov_jdwbs343p().f[76]++;const outerWidth=/* istanbul ignore next */(cov_jdwbs343p().s[342]++,env.outerWidth|0);const outerHeight=/* istanbul ignore next */(cov_jdwbs343p().s[343]++,env.outerHeight|0);const lineHeight=/* istanbul ignore next */(cov_jdwbs343p().s[344]++,env.lineHeight|0);const lineNumbersDigitCount=/* istanbul ignore next */(cov_jdwbs343p().s[345]++,env.lineNumbersDigitCount|0);const typicalHal