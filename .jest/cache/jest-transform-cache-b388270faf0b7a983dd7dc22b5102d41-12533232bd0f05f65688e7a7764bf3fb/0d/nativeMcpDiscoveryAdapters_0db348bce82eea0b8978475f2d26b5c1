  if (!coverage[path] || coverage[path].hash !== hash) {
    coverage[path] = coverageData;
  }
  var actualCoverage = coverage[path];
  {
    // @ts-ignore
    cov_sk1wbbg8o = function () {
      return actualCoverage;
    };
  }
  return actualCoverage;
}
