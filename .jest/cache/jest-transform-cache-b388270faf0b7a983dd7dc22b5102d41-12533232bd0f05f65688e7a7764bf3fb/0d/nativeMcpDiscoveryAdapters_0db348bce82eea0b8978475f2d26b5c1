export async function claudeConfigToServerDefinition(idPrefix, contents, cwd) {
  /* istanbul ignore next */
  cov_sk1wbbg8o().f[0]++;
  let parsed;
  /* istanbul ignore next */
  cov_sk1wbbg8o().s[0]++;
  try {
    /* istanbul ignore next */
    cov_sk1wbbg8o().s[1]++;
    parsed = JSON.parse(contents.toString());
  } catch {
    /* istanbul ignore next */
    cov_sk1wbbg8o().s[2]++;
    return;
  }
  /* istanbul ignore next */
  cov_sk1wbbg8o().s[3]++;
  return Promise.all(Object.entries(parsed.mcpServers).map(async ([name, server]) => {
    /* istanbul ignore next */
    cov_sk1wbbg8o().f[1]++;
    const launch =
    /* istanbul ignore next */
    (cov_sk1wbbg8o().s[4]++, server.url ?
    /* istanbul ignore next */
    (cov_sk1wbbg8o().b[0][0]++, {
      type: 2 /* McpServerTransportType.HTTP */,
      uri: URI.parse(server.url),
      headers: []
    }) :
    /* istanbul ignore next */
    (cov_sk1wbbg8o().b[0][1]++, {
      type: 1 /* McpServerTransportType.Stdio */,
      args:
      /* istanbul ignore next */
      (cov_sk1wbbg8o().b[1][0]++, server.args) ||
      /* istanbul ignore next */
      (cov_sk1wbbg8o().b[1][1]++, []),
      command: server.command,
      env:
      /* istanbul ignore next */
      (cov_sk1wbbg8o().b[2][0]++, server.env) ||
      /* istanbul ignore next */
      (cov_sk1wbbg8o().b[2][1]++, {}),
      envFile: undefined,
      cwd: cwd?.fsPath
    }));
    /* istanbul ignore next */
    cov_sk1wbbg8o().s[5]++;
    return {
      id: `${idPrefix}.${name}`,
      label: name,
      launch,
      cacheNonce: await McpServerLaunch.hash(launch)
    };
  }));
}
