      (cov_bcnj979v9().s[49]++, '');
      /* istanbul ignore next */
      cov_bcnj979v9().s[50]++;
      rgProc.stderr.on('data', data => {
        /* istanbul ignore next */
        cov_bcnj979v9().f[15]++;
        const message =
        /* istanbul ignore next */
        (cov_bcnj979v9().s[51]++, data.toString());
        /* istanbul ignore next */
        cov_bcnj979v9().s[52]++;
        this.outputChannel.appendLine(message);
        /* istanbul ignore next */
        cov_bcnj979v9().s[53]++;
        if (stderr.length + message.length < 1e6) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[7][0]++;
          cov_bcnj979v9().s[54]++;
          stderr += message;
        } else
        /* istanbul ignore next */
        {
          cov_bcnj979v9().b[7][1]++;
        }
      });
      /* istanbul ignore next */
      cov_bcnj979v9().s[55]++;
      rgProc.on('close', () => {
        /* istanbul ignore next */
        cov_bcnj979v9().f[16]++;
        cov_bcnj979v9().s[56]++;
        this.outputChannel.appendLine(gotData ?
        /* istanbul ignore next */
        (cov_bcnj979v9().b[8][0]++, 'Got data from stdout') :
        /* istanbul ignore next */
        (cov_bcnj979v9().b[8][1]++, 'No data from stdout'));
        /* istanbul ignore next */
        cov_bcnj979v9().s[57]++;
        this.outputChannel.appendLine(gotResult ?
        /* istanbul ignore next */
        (cov_bcnj979v9().b[9][0]++, 'Got result from parser') :
        /* istanbul ignore next */
        (cov_bcnj979v9().b[9][1]++, 'No result from parser'));
        /* istanbul ignore next */
        cov_bcnj979v9().s[58]++;
        if (dataWithoutResult) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[10][0]++;
          cov_bcnj979v9().s[59]++;
          this.outputChannel.appendLine(`Got data without result: ${dataWithoutResult}`);
        } else
        /* istanbul ignore next */
        {
          cov_bcnj979v9().b[10][1]++;
        }
        cov_bcnj979v9().s[60]++;
        this.outputChannel.appendLine('');
        /* istanbul ignore next */
        cov_bcnj979v9().s[61]++;
        if (isDone) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[11][0]++;
          cov_bcnj979v9().s[62]++;
          resolve({
            limitHit
          });
        } else {
          /* istanbul ignore next */
          cov_bcnj979v9().b[11][1]++;
          cov_bcnj979v9().s[63]++;
          // Trigger last result
          ripgrepParser.flush();
          /* istanbul ignore next */
          cov_bcnj979v9().s[64]++;
          rgProc = null;
          let searchError;
          /* istanbul ignore next */
          cov_bcnj979v9().s[65]++;
          if (
          /* istanbul ignore next */
          (cov_bcnj979v9().b[13][0]++, stderr) &&
          /* istanbul ignore next */
          (cov_bcnj979v9().b[13][1]++, !gotData) &&
          /* istanbul ignore next */
          (cov_bcnj979v9().b[13][2]++, searchError = rgErrorMsgForDisplay(stderr))) {
            /* istanbul ignore next */
            cov_bcnj979v9().b[12][0]++;
            cov_bcnj979v9().s[66]++;
            reject(serializeSearchError(new SearchError(searchError.message, searchError.code)));
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[12][1]++;
            cov_bcnj979v9().s[67]++;
            resolve({
              limitHit
            });
          }
        }
      });
    });
  }
}
/**
 * Read the first line of stderr and return an error for display or undefined, based on a list of
 * allowed properties.
 * Ripgrep produces stderr output which is not from a fatal error, and we only want the search to be
 * "failed" when a fatal error was produced.
 */
function rgErrorMsgForDisplay(msg) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[17]++;
  const lines =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[68]++, msg.split('\n'));
  const firstLine =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[69]++, lines[0].trim());
  /* istanbul ignore next */
  cov_bcnj979v9().s[70]++;
  if (lines.some(l => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[18]++;
    cov_bcnj979v9().s[71]++;
    return l.startsWith('regex parse error');
  })) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[14][0]++;
    cov_bcnj979v9().s[72]++;
    return new SearchError(buildRegexParseError(lines), SearchErrorCode.regexParseError);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[14][1]++;
  }
  const match =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[73]++, firstLine.match(/grep config error: unknown encoding: (.*)/));
  /* istanbul ignore next */
  cov_bcnj979v9().s[74]++;
  if (match) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[15][0]++;
    cov_bcnj979v9().s[75]++;
    return new SearchError(`Unknown encoding: ${match[1]}`, SearchErrorCode.unknownEncoding);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[15][1]++;
  }
  cov_bcnj979v9().s[76]++;
  if (firstLine.startsWith('error parsing glob')) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[16][0]++;
    cov_bcnj979v9().s[77]++;
    // Uppercase first letter
    return new SearchError(firstLine.charAt(0).toUpperCase() + firstLine.substr(1), SearchErrorCode.globParseError);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[16][1]++;
  }
  cov_bcnj979v9().s[78]++;
  if (firstLine.startsWith('the literal')) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[17][0]++;
    cov_bcnj979v9().s[79]++;
    // Uppercase first letter
    return new SearchError(firstLine.charAt(0).toUpperCase() + firstLine.substr(1), SearchErrorCode.invalidLiteral);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[17][1]++;
  }
  cov_bcnj979v9().s[80]++;
  if (firstLine.startsWith('PCRE2: error compiling pattern')) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[18][0]++;
    cov_bcnj979v9().s[81]++;
    return new SearchError(firstLine, SearchErrorCode.regexParseError);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[18][1]++;
  }
  cov_bcnj979v9().s[82]++;
  return undefined;
}
function buildRegexParseError(lines) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[19]++;
  const errorMessage =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[83]++, ['Regex parse error']);
  const pcre2ErrorLine =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[84]++, lines.filter(l => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[20]++;
    cov_bcnj979v9().s[85]++;
    return l.startsWith('PCRE2:');
  }));
  /* istanbul ignore next */
  cov_bcnj979v9().s[86]++;
  if (pcre2ErrorLine.length >= 1) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[19][0]++;
    const pcre2ErrorMessage =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[87]++, pcre2ErrorLine[0].replace('PCRE2:', ''));
    /* istanbul ignore next */
    cov_bcnj979v9().s[88]++;
    if (
    /* istanbul ignore next */
    (cov_bcnj979v9().b[21][0]++, pcre2ErrorMessage.indexOf(':') !== -1) &&
    /* istanbul ignore next */
    (cov_bcnj979v9().b[21][1]++, pcre2ErrorMessage.split(':').length >= 2)) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[20][0]++;
      const pcre2ActualErrorMessage =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[89]++, pcre2ErrorMessage.split(':')[1]);
      /* istanbul ignore next */
      cov_bcnj979v9().s[90]++;
      errorMessage.push(':' + pcre2ActualErrorMessage);
    } else
    /* istanbul ignore next */
    {
      cov_bcnj979v9().b[20][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[19][1]++;
  }
  cov_bcnj979v9().s[91]++;
  return errorMessage.join('');
}
export class RipgrepParser extends EventEmitter {
  maxResults;
  root;
  previewOptions;
  remainder =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[92]++, '');
  isDone =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[93]++, false);
  hitLimit =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[94]++, false);
  stringDecoder;
  numResults =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[95]++, 0);
  constructor(maxResults, root, previewOptions) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[21]++;
    cov_bcnj979v9().s[96]++;
    super();
    /* istanbul ignore next */
    cov_bcnj979v9().s[97]++;
    this.maxResults = maxResults;
    /* istanbul ignore next */
    cov_bcnj979v9().s[98]++;
    this.root = root;
    /* istanbul ignore next */
    cov_bcnj979v9().s[99]++;
    this.previewOptions = previewOptions;
    /* istanbul ignore next */
    cov_bcnj979v9().s[100]++;
    this.stringDecoder = new StringDecoder();
  }
  cancel() {
    /* istanbul ignore next */
    cov_bcnj979v9().f[22]++;
    cov_bcnj979v9().s[101]++;
    this.isDone = true;
  }
  flush() {
    /* istanbul ignore next */
    cov_bcnj979v9().f[23]++;
    cov_bcnj979v9().s[102]++;
    this.handleDecodedData(this.stringDecoder.end());
  }
  on(event, listener) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[24]++;
    cov_bcnj979v9().s[103]++;
    super.on(event, listener);
    /* istanbul ignore next */
    cov_bcnj979v9().s[104]++;
    return this;
  }
  handleData(data) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[25]++;
    cov_bcnj979v9().s[105]++;
    if (this.isDone) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[22][0]++;
      cov_bcnj979v9().s[106]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_bcnj979v9().b[22][1]++;
    }
    const dataStr =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[107]++, typeof data === 'string' ?
    /* istanbul ignore next */
    (cov_bcnj979v9().b[23][0]++, data) :
    /* istanbul ignore next */
    (cov_bcnj979v9().b[23][1]++, this.stringDecoder.write(data)));
    /* istanbul ignore next */
    cov_bcnj979v9().s[108]++;
    this.handleDecodedData(dataStr);
  }
  handleDecodedData(decodedData) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[26]++;
    // check for newline before appending to remainder
    let newlineIdx =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[109]++, decodedData.indexOf('\n'));
    // If the previous data chunk didn't end in a newline, prepend it to this chunk
    const dataStr =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[110]++, this.remainder + decodedData);
    /* istanbul ignore next */
    cov_bcnj979v9().s[111]++;
    if (newlineIdx >= 0) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[24][0]++;
      cov_bcnj979v9().s[112]++;
      newlineIdx += this.remainder.length;
    } else {
      /* istanbul ignore next */
      cov_bcnj979v9().b[24][1]++;
      cov_bcnj979v9().s[113]++;
      // Shortcut
      this.remainder = dataStr;
      /* istanbul ignore next */
      cov_bcnj979v9().s[114]++;
      return;
    }
    let prevIdx =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[115]++, 0);
    /* istanbul ignore next */
    cov_bcnj979v9().s[116]++;
    while (newlineIdx >= 0) {
      /* istanbul ignore next */
      cov_bcnj979v9().s[117]++;
      this.handleLine(dataStr.substring(prevIdx, newlineIdx).trim());
      /* istanbul ignore next */
      cov_bcnj979v9().s[118]++;
      prevIdx = newlineIdx + 1;
      /* istanbul ignore next */
      cov_bcnj979v9().s[119]++;
      newlineIdx = dataStr.indexOf('\n', prevIdx);
    }
    /* istanbul ignore next */
    cov_bcnj979v9().s[120]++;
    this.remainder = dataStr.substring(prevIdx);
  }
  handleLine(outputLine) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[27]++;
    cov_bcnj979v9().s[121]++;
    if (
    /* istanbul ignore next */
    (cov_bcnj979v9().b[26][0]++, this.isDone) ||
    /* istanbul ignore next */
    (cov_bcnj979v9().b[26][1]++, !outputLine)) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[25][0]++;
      cov_bcnj979v9().s[122]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_bcnj979v9().b[25][1]++;
    }
    let parsedLine;
    /* istanbul ignore next */
    cov_bcnj979v9().s[123]++;
    try {
      /* istanbul ignore next */
      cov_bcnj979v9().s[124]++;
      parsedLine = JSON.parse(outputLine);
    } catch (e) {
      /* istanbul ignore next */
      cov_bcnj979v9().s[125]++;
      throw new Error(`malformed line from rg: ${outputLine}`);
    }
    /* istanbul ignore next */
    cov_bcnj979v9().s[126]++;
    if (parsedLine.type === 'match') {
      /* istanbul ignore next */
      cov_bcnj979v9().b[27][0]++;
      const matchPath =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[127]++, bytesOrTextToString(parsedLine.data.path));
      const uri =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[128]++, URI.joinPath(this.root, matchPath));
      const result =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[129]++, this.createTextSearchMatch(parsedLine.data, uri));
      /* istanbul ignore next */
      cov_bcnj979v9().s[130]++;
      this.onResult(result);
      /* istanbul ignore next */
      cov_bcnj979v9().s[131]++;
      if (this.hitLimit) {
        /* istanbul ignore next */
        cov_bcnj979v9().b[28][0]++;
        cov_bcnj979v9().s[132]++;
        this.cancel();
        /* istanbul ignore next */
        cov_bcnj979v9().s[133]++;
        this.emit('hitLimit');
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[28][1]++;
      }
    } else {
      /* istanbul ignore next */
      cov_bcnj979v9().b[27][1]++;
      cov_bcnj979v9().s[134]++;
      if (parsedLine.type === 'context') {
        /* istanbul ignore next */
        cov_bcnj979v9().b[29][0]++;
        const contextPath =
        /* istanbul ignore next */
        (cov_bcnj979v9().s[135]++, bytesOrTextToString(parsedLine.data.path));
        const uri =
        /* istanbul ignore next */
        (cov_bcnj979v9().s[136]++, URI.joinPath(this.root, contextPath));
        const result =
        /* istanbul ignore next */
        (cov_bcnj979v9().s[137]++, this.createTextSearchContexts(parsedLine.data, uri));
        /* istanbul ignore next */
        cov_bcnj979v9().s[138]++;
        result.forEach(r => {
          /* istanbul ignore next */
          cov_bcnj979v9().f[28]++;
          cov_bcnj979v9().s[139]++;
          return this.onResult(r);
        });
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[29][1]++;
      }
    }
  }
  createTextSearchMatch(data, uri) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[29]++;
    const lineNumber =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[140]++, data.line_number - 1);
    const fullText =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[141]++, bytesOrTextToString(data.lines));
    const fullTextBytes =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[142]++, Buffer.from(fullText));
    let prevMatchEnd =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[143]++, 0);
    let prevMatchEndCol =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[144]++, 0);
    let prevMatchEndLine =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[145]++, lineNumber);
    // it looks like certain regexes can match a line, but cause rg to not
    // emit any specific submatches for that line.
    // https://github.com/microsoft/vscode/issues/100569#issuecomment-738496991
    /* istanbul ignore next */
    cov_bcnj979v9().s[146]++;
    if (data.submatches.length === 0) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[30][0]++;
      cov_bcnj979v9().s[147]++;
      data.submatches.push(fullText.length ?
      /* istanbul ignore next */
      (cov_bcnj979v9().b[31][0]++, {
        start: 0,
        end: 1,
        match: {
          text: fullText[0]
        }
      }) :
      /* istanbul ignore next */
      (cov_bcnj979v9().b[31][1]++, {
        start: 0,
        end: 0,
        match: {
          text: ''
        }
      }));
    } else
    /* istanbul ignore next */
    {
      cov_bcnj979v9().b[30][1]++;
    }
    const ranges =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[148]++, coalesce(data.submatches.map((match, i) => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[30]++;
      cov_bcnj979v9().s[149]++;
      if (this.hitLimit) {
        /* istanbul ignore next */
        cov_bcnj979v9().b[32][0]++;
        cov_bcnj979v9().s[150]++;
        return null;
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[32][1]++;
      }
      cov_bcnj979v9().s[151]++;
      this.numResults++;
      /* istanbul ignore next */
      cov_bcnj979v9().s[152]++;
      if (this.numResults >= this.maxResults) {
        /* istanbul ignore next */
        cov_bcnj979v9().b[33][0]++;
        cov_bcnj979v9().s[153]++;
        // Finish the line, then report the result below
        this.hitLimit = true;
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[33][1]++;
      }
      const matchText =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[154]++, bytesOrTextToString(match.match));
      const inBetweenText =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[155]++, fullTextBytes.slice(prevMatchEnd, match.start).toString());
      const inBetweenStats =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[156]++, getNumLinesAndLastNewlineLength(inBetweenText));
      const startCol =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[157]++, inBetweenStats.numLines > 0 ?
      /* istanbul ignore next */
      (cov_bcnj979v9().b[34][0]++, inBetweenStats.lastLineLength) :
      /* istanbul ignore next */
      (cov_bcnj979v9().b[34][1]++, inBetweenStats.lastLineLength + prevMatchEndCol));
      const stats =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[158]++, getNumLinesAndLastNewlineLength(matchText));
      const startLineNumber =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[159]++, inBetweenStats.numLines + prevMatchEndLine);
      const endLineNumber =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[160]++, stats.numLines + startLineNumber);
      const endCol =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[161]++, stats.numLines > 0 ?
      /* istanbul ignore next */
      (cov_bcnj979v9().b[35][0]++, stats.lastLineLength) :
      /* istanbul ignore next */
      (cov_bcnj979v9().b[35][1]++, stats.lastLineLength + startCol));
      /* istanbul ignore next */
      cov_bcnj979v9().s[162]++;
      prevMatchEnd = match.end;
      /* istanbul ignore next */
      cov_bcnj979v9().s[163]++;
      prevMatchEndCol = endCol;
      /* istanbul ignore next */
      cov_bcnj979v9().s[164]++;
      prevMatchEndLine = endLineNumber;
      /* istanbul ignore next */
      cov_bcnj979v9().s[165]++;
      return new Range(startLineNumber, startCol, endLineNumber, endCol);
    })));
    const searchRange =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[166]++, mapArrayOrNot(ranges, rangeToSearchRange));
    const internalResult =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[167]++, new TextSearchMatch(fullText, searchRange, this.previewOptions));
    /* istanbul ignore next */
    cov_bcnj979v9().s[168]++;
    return new TextSearchMatch2(uri, internalResult.rangeLocations.map(e => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[31]++;
      cov_bcnj979v9().s[169]++;
      return {
        sourceRange: searchRangeToRange(e.source),
        previewRange: searchRangeToRange(e.preview)
      };
    }), internalResult.previewText);
  }
  createTextSearchContexts(data, uri) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[32]++;
    const text =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[170]++, bytesOrTextToString(data.lines));
    const startLine =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[171]++, data.line_number);
    /* istanbul ignore next */
    cov_bcnj979v9().s[172]++;
    return text.replace(/\r?\n$/, '').split('\n').map((line, i) => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[33]++;
      cov_bcnj979v9().s[173]++;
      return new TextSearchContext2(uri, line, startLine + i);
    });
  }
  onResult(match) {
    /* istanbul ignore next */
    cov_bcnj979v9().f[34]++;
    cov_bcnj979v9().s[174]++;
    this.emit('result', match);
  }
}
function bytesOrTextToString(obj) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[35]++;
  cov_bcnj979v9().s[175]++;
  return obj.bytes ?
  /* istanbul ignore next */
  (cov_bcnj979v9().b[36][0]++, Buffer.from(obj.bytes, 'base64').toString()) :
  /* istanbul ignore next */
  (cov_bcnj979v9().b[36][1]++, obj.text);
}
function getNumLinesAndLastNewlineLength(text) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[36]++;
  const re =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[176]++, /\n/g);
  let numLines =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[177]++, 0);
  let lastNewlineIdx =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[178]++, -1);
  let match;
  /* istanbul ignore next */
  cov_bcnj979v9().s[179]++;
  while (match = re.exec(text)) {
    /* istanbul ignore next */
    cov_bcnj979v9().s[180]++;
    numLines++;
    /* istanbul ignore next */
    cov_bcnj979v9().s[181]++;
    lastNewlineIdx = match.index;
  }
  const lastLineLength =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[182]++, lastNewlineIdx >= 0 ?
  /* istanbul ignore next */
  (cov_bcnj979v9().b[37][0]++, text.length - lastNewlineIdx - 1) :
  /* istanbul ignore next */
  (cov_bcnj979v9().b[37][1]++, text.length));
  /* istanbul ignore next */
  cov_bcnj979v9().s[183]++;
  return {
    numLines,
    lastLineLength
  };
}
// exported for testing
export function getRgArgs(query, options) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[37]++;
  const args =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[184]++, ['--hidden', '--no-require-git']);
  /* istanbul ignore next */
  cov_bcnj979v9().s[185]++;
  args.push(query.isCaseSensitive ?
  /* istanbul ignore next */
  (cov_bcnj979v9().b[38][0]++, '--case-sensitive') :
  /* istanbul ignore next */
  (cov_bcnj979v9().b[38][1]++, '--ignore-case'));
  const {
    doubleStarIncludes,
    otherIncludes
  } =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[186]++, groupBy(options.folderOptions.includes, include => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[38]++;
    cov_bcnj979v9().s[187]++;
    return include.startsWith('**') ?
    /* istanbul ignore next */
    (cov_bcnj979v9().b[39][0]++, 'doubleStarIncludes') :
    /* istanbul ignore next */
    (cov_bcnj979v9().b[39][1]++, 'otherIncludes');
  }));
  /* istanbul ignore next */
  cov_bcnj979v9().s[188]++;
  if (
  /* istanbul ignore next */
  (cov_bcnj979v9().b[41][0]++, otherIncludes) &&
  /* istanbul ignore next */
  (cov_bcnj979v9().b[41][1]++, otherIncludes.length)) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[40][0]++;
    const uniqueOthers =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[189]++, new Set());
    /* istanbul ignore next */
    cov_bcnj979v9().s[190]++;
    otherIncludes.forEach(other => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[39]++;
      cov_bcnj979v9().s[191]++;
      uniqueOthers.add(other);
    });
    /* istanbul ignore next */
    cov_bcnj979v9().s[192]++;
    args.push('-g', '!*');
    /* istanbul ignore next */
    cov_bcnj979v9().s[193]++;
    uniqueOthers.forEach(otherIncude => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[40]++;
      cov_bcnj979v9().s[194]++;
      spreadGlobComponents(otherIncude).map(anchorGlob).forEach(globArg => {
        /* istanbul ignore next */
        cov_bcnj979v9().f[41]++;
        cov_bcnj979v9().s[195]++;
        args.push('-g', globArg);
      });
    });
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[40][1]++;
  }
  cov_bcnj979v9().s[196]++;
  if (
  /* istanbul ignore next */
  (cov_bcnj979v9().b[43][0]++, doubleStarIncludes) &&
  /* istanbul ignore next */
  (cov_bcnj979v9().b[43][1]++, doubleStarIncludes.length)) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[42][0]++;
    cov_bcnj979v9().s[197]++;
    doubleStarIncludes.forEach(globArg => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[42]++;
      cov_bcnj979v9().s[198]++;
      args.push('-g', globArg);
    });
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[42][1]++;
  }
  cov_bcnj979v9().s[199]++;
  options.folderOptions.excludes.map(e => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[43]++;
    cov_bcnj979v9().s[200]++;
    return typeof e === 'string' ?
    /* istanbul ignore next */
    (cov_bcnj979v9().b[44][0]++, e) :
    /* istanbul ignore next */
    (cov_bcnj979v9().b[44][1]++, e.pattern);
  }).map(anchorGlob).forEach(rgGlob => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[44]++;
    cov_bcnj979v9().s[201]++;
    return args.push('-g', `!${rgGlob}`);
  });
  /* istanbul ignore next */
  cov_bcnj979v9().s[202]++;
  if (options.maxFileSize) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[45][0]++;
    cov_bcnj979v9().s[203]++;
    args.push('--max-filesize', options.maxFileSize + '');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[45][1]++;
  }
  cov_bcnj979v9().s[204]++;
  if (options.folderOptions.useIgnoreFiles.local) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[46][0]++;
    cov_bcnj979v9().s[205]++;
    if (!options.folderOptions.useIgnoreFiles.parent) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[47][0]++;
      cov_bcnj979v9().s[206]++;
      args.push('--no-ignore-parent');
    } else
    /* istanbul ignore next */
    {
      cov_bcnj979v9().b[47][1]++;
    }
  } else {
    /* istanbul ignore next */
    cov_bcnj979v9().b[46][1]++;
    cov_bcnj979v9().s[207]++;
    // Don't use .gitignore or .ignore
    args.push('--no-ignore');
  }
  /* istanbul ignore next */
  cov_bcnj979v9().s[208]++;
  if (options.folderOptions.followSymlinks) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[48][0]++;
    cov_bcnj979v9().s[209]++;
    args.push('--follow');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[48][1]++;
  }
  cov_bcnj979v9().s[210]++;
  if (
  /* istanbul ignore next */
  (cov_bcnj979v9().b[50][0]++, options.folderOptions.encoding) &&
  /* istanbul ignore next */
  (cov_bcnj979v9().b[50][1]++, options.folderOptions.encoding !== 'utf8')) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[49][0]++;
    cov_bcnj979v9().s[211]++;
    args.push('--encoding', options.folderOptions.encoding);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[49][1]++;
  }
  cov_bcnj979v9().s[212]++;
  if (options.numThreads) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[51][0]++;
    cov_bcnj979v9().s[213]++;
    args.push('--threads', `${options.numThreads}`);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[51][1]++;
  }
  // Ripgrep handles -- as a -- arg separator. Only --.
  // - is ok, --- is ok, --some-flag is also ok. Need to special case.
  cov_bcnj979v9().s[214]++;
  if (query.pattern === '--') {
    /* istanbul ignore next */
    cov_bcnj979v9().b[52][0]++;
    cov_bcnj979v9().s[215]++;
    query.isRegExp = true;
    /* istanbul ignore next */
    cov_bcnj979v9().s[216]++;
    query.pattern = '\\-\\-';
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[52][1]++;
  }
  cov_bcnj979v9().s[217]++;
  if (
  /* istanbul ignore next */
  (cov_bcnj979v9().b[54][0]++, query.isMultiline) &&
  /* istanbul ignore next */
  (cov_bcnj979v9().b[54][1]++, !query.isRegExp)) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[53][0]++;
    cov_bcnj979v9().s[218]++;
    query.pattern = escapeRegExpCharacters(query.pattern);
    /* istanbul ignore next */
    cov_bcnj979v9().s[219]++;
    query.isRegExp = true;
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[53][1]++;
  }
  cov_bcnj979v9().s[220]++;
  if (options.usePCRE2) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[55][0]++;
    cov_bcnj979v9().s[221]++;
    args.push('--pcre2');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[55][1]++;
  }
  // Allow $ to match /r/n
  cov_bcnj979v9().s[222]++;
  args.push('--crlf');
  /* istanbul ignore next */
  cov_bcnj979v9().s[223]++;
  if (query.isRegExp) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[56][0]++;
    cov_bcnj979v9().s[224]++;
    query.pattern = unicodeEscapesToPCRE2(query.pattern);
    /* istanbul ignore next */
    cov_bcnj979v9().s[225]++;
    args.push('--engine', 'auto');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[56][1]++;
  }
  let searchPatternAfterDoubleDashes;
  /* istanbul ignore next */
  cov_bcnj979v9().s[226]++;
  if (query.isWordMatch) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[57][0]++;
    const regexp =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[227]++, createRegExp(query.pattern, !!query.isRegExp, {
      wholeWord: query.isWordMatch
    }));
    const regexpStr =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[228]++, regexp.source.replace(/\\\//g, '/')); // RegExp.source arbitrarily returns escaped slashes. Search and destroy.
    /* istanbul ignore next */
    cov_bcnj979v9().s[229]++;
    args.push('--regexp', regexpStr);
  } else {
    /* istanbul ignore next */
    cov_bcnj979v9().b[57][1]++;
    cov_bcnj979v9().s[230]++;
    if (query.isRegExp) {
      /* istanbul ignore next */
      cov_bcnj979v9().b[58][0]++;
      let fixedRegexpQuery =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[231]++, fixRegexNewline(query.pattern));
      /* istanbul ignore next */
      cov_bcnj979v9().s[232]++;
      fixedRegexpQuery = fixNewline(fixedRegexpQuery);
      /* istanbul ignore next */
      cov_bcnj979v9().s[233]++;
      args.push('--regexp', fixedRegexpQuery);
    } else {
      /* istanbul ignore next */
      cov_bcnj979v9().b[58][1]++;
      cov_bcnj979v9().s[234]++;
      searchPatternAfterDoubleDashes = query.pattern;
      /* istanbul ignore next */
      cov_bcnj979v9().s[235]++;
      args.push('--fixed-strings');
    }
  }
  /* istanbul ignore next */
  cov_bcnj979v9().s[236]++;
  args.push('--no-config');
  /* istanbul ignore next */
  cov_bcnj979v9().s[237]++;
  if (!options.folderOptions.useIgnoreFiles.global) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[59][0]++;
    cov_bcnj979v9().s[238]++;
    args.push('--no-ignore-global');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[59][1]++;
  }
  cov_bcnj979v9().s[239]++;
  args.push('--json');
  /* istanbul ignore next */
  cov_bcnj979v9().s[240]++;
  if (query.isMultiline) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[60][0]++;
    cov_bcnj979v9().s[241]++;
    args.push('--multiline');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[60][1]++;
  }
  cov_bcnj979v9().s[242]++;
  if (options.surroundingContext) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[61][0]++;
    cov_bcnj979v9().s[243]++;
    args.push('--before-context', options.surroundingContext + '');
    /* istanbul ignore next */
    cov_bcnj979v9().s[244]++;
    args.push('--after-context', options.surroundingContext + '');
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[61][1]++;
  }
  // Folder to search
  cov_bcnj979v9().s[245]++;
  args.push('--');
  /* istanbul ignore next */
  cov_bcnj979v9().s[246]++;
  if (searchPatternAfterDoubleDashes) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[62][0]++;
    cov_bcnj979v9().s[247]++;
    // Put the query after --, in case the query starts with a dash
    args.push(searchPatternAfterDoubleDashes);
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[62][1]++;
  }
  cov_bcnj979v9().s[248]++;
  args.push('.');
  /* istanbul ignore next */
  cov_bcnj979v9().s[249]++;
  return args;
}
/**
 * `"foo/*bar/something"` -> `["foo", "foo/*bar", "foo/*bar/something", "foo/*bar/something/**"]`
 */
function spreadGlobComponents(globComponent) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[45]++;
  const globComponentWithBraceExpansion =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[250]++, performBraceExpansionForRipgrep(globComponent));
  /* istanbul ignore next */
  cov_bcnj979v9().s[251]++;
  return globComponentWithBraceExpansion.flatMap(globArg => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[46]++;
    const components =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[252]++, splitGlobAware(globArg, '/'));
    /* istanbul ignore next */
    cov_bcnj979v9().s[253]++;
    return components.map((_, i) => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[47]++;
      cov_bcnj979v9().s[254]++;
      return components.slice(0, i + 1).join('/');
    });
  });
}
export function unicodeEscapesToPCRE2(pattern) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[48]++;
  // Match \u1234
  const unicodePattern =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[255]++, /((?:[^\\]|^)(?:\\\\)*)\\u([a-z0-9]{4})/gi);
  /* istanbul ignore next */
  cov_bcnj979v9().s[256]++;
  while (pattern.match(unicodePattern)) {
    /* istanbul ignore next */
    cov_bcnj979v9().s[257]++;
    pattern = pattern.replace(unicodePattern, `$1\\x{$2}`);
  }
  // Match \u{1234}
  // \u with 5-6 characters will be left alone because \x only takes 4 characters.
  const unicodePatternWithBraces =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[258]++, /((?:[^\\]|^)(?:\\\\)*)\\u\{([a-z0-9]{4})\}/gi);
  /* istanbul ignore next */
  cov_bcnj979v9().s[259]++;
  while (pattern.match(unicodePatternWithBraces)) {
    /* istanbul ignore next */
    cov_bcnj979v9().s[260]++;
    pattern = pattern.replace(unicodePatternWithBraces, `$1\\x{$2}`);
  }
  /* istanbul ignore next */
  cov_bcnj979v9().s[261]++;
  return pattern;
}
/* istanbul ignore next */
cov_bcnj979v9().s[262]++;
const isLookBehind = node => {
  /* istanbul ignore next */
  cov_bcnj979v9().f[49]++;
  cov_bcnj979v9().s[263]++;
  return /* istanbul ignore next */(cov_bcnj979v9().b[63][0]++, node.type === 'Assertion') &&
  /* istanbul ignore next */
  (cov_bcnj979v9().b[63][1]++, node.kind === 'lookbehind');
};
export function fixRegexNewline(pattern) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[50]++;
  // we parse the pattern anew each tiem
  let re;
  /* istanbul ignore next */
  cov_bcnj979v9().s[264]++;
  try {
    /* istanbul ignore next */
    cov_bcnj979v9().s[265]++;
    re = new RegExpParser().parsePattern(pattern);
  } catch {
    /* istanbul ignore next */
    cov_bcnj979v9().s[266]++;
    return pattern;
  }
  let output =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[267]++, '');
  let lastEmittedIndex =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[268]++, 0);
  /* istanbul ignore next */
  cov_bcnj979v9().s[269]++;
  const replace = (start, end, text) => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[51]++;
    cov_bcnj979v9().s[270]++;
    output += pattern.slice(lastEmittedIndex, start) + text;
    /* istanbul ignore next */
    cov_bcnj979v9().s[271]++;
    lastEmittedIndex = end;
  };
  const context =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[272]++, []);
  const visitor =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[273]++, new RegExpVisitor({
    onCharacterEnter(char) {
      /* istanbul ignore next */
      cov_bcnj979v9().f[52]++;
      cov_bcnj979v9().s[274]++;
      if (char.raw !== '\\n') {
        /* istanbul ignore next */
        cov_bcnj979v9().b[64][0]++;
        cov_bcnj979v9().s[275]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[64][1]++;
      }
      const parent =
      /* istanbul ignore next */
      (cov_bcnj979v9().s[276]++, context[0]);
      /* istanbul ignore next */
      cov_bcnj979v9().s[277]++;
      if (!parent) {
        /* istanbul ignore next */
        cov_bcnj979v9().b[65][0]++;
        cov_bcnj979v9().s[278]++;
        // simple char, \n -> \r?\n
        replace(char.start, char.end, '\\r?\\n');
      } else {
        /* istanbul ignore next */
        cov_bcnj979v9().b[65][1]++;
        cov_bcnj979v9().s[279]++;
        if (context.some(isLookBehind)) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[66][0]++;
        } // no-op in a lookbehind, see #100569
        else {
          /* istanbul ignore next */
          cov_bcnj979v9().b[66][1]++;
          cov_bcnj979v9().s[280]++;
          if (parent.type === 'CharacterClass') {
            /* istanbul ignore next */
            cov_bcnj979v9().b[67][0]++;
            cov_bcnj979v9().s[281]++;
            if (parent.negate) {
              /* istanbul ignore next */
              cov_bcnj979v9().b[68][0]++;
              // negative bracket expr, [^a-z\n] -> (?![a-z]|\r?\n)
              const otherContent =
              /* istanbul ignore next */
              (cov_bcnj979v9().s[282]++, pattern.slice(parent.start + 2, char.start) + pattern.slice(char.end, parent.end - 1));
              /* istanbul ignore next */
              cov_bcnj979v9().s[283]++;
              if (parent.parent?.type === 'Quantifier') {
                /* istanbul ignore next */
                cov_bcnj979v9().b[69][0]++;
                cov_bcnj979v9().s[284]++;
                // If quantified, we can't use a negative lookahead in a quantifier.
                // But `.` already doesn't match new lines, so we can just use that
                // (with any other negations) instead.
                replace(parent.start, parent.end, otherContent ?
                /* istanbul ignore next */
                (cov_bcnj979v9().b[70][0]++, `[^${otherContent}]`) :
                /* istanbul ignore next */
                (cov_bcnj979v9().b[70][1]++, '.'));
              } else {
                /* istanbul ignore next */
                cov_bcnj979v9().b[69][1]++;
                cov_bcnj979v9().s[285]++;
                replace(parent.start, parent.end, '(?!\\r?\\n' + (otherContent ?
                /* istanbul ignore next */
                (cov_bcnj979v9().b[71][0]++, `|[${otherContent}]`) :
                /* istanbul ignore next */
                (cov_bcnj979v9().b[71][1]++, '')) + ')');
              }
            } else {
              /* istanbul ignore next */
              cov_bcnj979v9().b[68][1]++;
              // positive bracket expr, [a-z\n] -> (?:[a-z]|\r?\n)
              const otherContent =
              /* istanbul ignore next */
              (cov_bcnj979v9().s[286]++, pattern.slice(parent.start + 1, char.start) + pattern.slice(char.end, parent.end - 1));
              /* istanbul ignore next */
              cov_bcnj979v9().s[287]++;
              replace(parent.start, parent.end, otherContent === '' ?
              /* istanbul ignore next */
              (cov_bcnj979v9().b[72][0]++, '\\r?\\n') :
              /* istanbul ignore next */
              (cov_bcnj979v9().b[72][1]++, `(?:[${otherContent}]|\\r?\\n)`));
            }
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[67][1]++;
            cov_bcnj979v9().s[288]++;
            if (parent.type === 'Quantifier') {
              /* istanbul ignore next */
              cov_bcnj979v9().b[73][0]++;
              cov_bcnj979v9().s[289]++;
              replace(char.start, char.end, '(?:\\r?\\n)');
            } else
            /* istanbul ignore next */
            {
              cov_bcnj979v9().b[73][1]++;
            }
          }
        }
      }
    },
    onQuantifierEnter(node) {
      /* istanbul ignore next */
      cov_bcnj979v9().f[53]++;
      cov_bcnj979v9().s[290]++;
      context.unshift(node);
    },
    onQuantifierLeave() {
      /* istanbul ignore next */
      cov_bcnj979v9().f[54]++;
      cov_bcnj979v9().s[291]++;
      context.shift();
    },
    onCharacterClassRangeEnter(node) {
      /* istanbul ignore next */
      cov_bcnj979v9().f[55]++;
      cov_bcnj979v9().s[292]++;
      context.unshift(node);
    },
    onCharacterClassRangeLeave() {
      /* istanbul ignore next */
      cov_bcnj979v9().f[56]++;
      cov_bcnj979v9().s[293]++;
      context.shift();
    },
    onCharacterClassEnter(node) {
      /* istanbul ignore next */
      cov_bcnj979v9().f[57]++;
      cov_bcnj979v9().s[294]++;
      context.unshift(node);
    },
    onCharacterClassLeave() {
      /* istanbul ignore next */
      cov_bcnj979v9().f[58]++;
      cov_bcnj979v9().s[295]++;
      context.shift();
    },
    onAssertionEnter(node) {
      /* istanbul ignore next */
      cov_bcnj979v9().f[59]++;
      cov_bcnj979v9().s[296]++;
      if (isLookBehind(node)) {
        /* istanbul ignore next */
        cov_bcnj979v9().b[74][0]++;
        cov_bcnj979v9().s[297]++;
        context.push(node);
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[74][1]++;
      }
    },
    onAssertionLeave(node) {
      /* istanbul ignore next */
      cov_bcnj979v9().f[60]++;
      cov_bcnj979v9().s[298]++;
      if (context[0] === node) {
        /* istanbul ignore next */
        cov_bcnj979v9().b[75][0]++;
        cov_bcnj979v9().s[299]++;
        context.shift();
      } else
      /* istanbul ignore next */
      {
        cov_bcnj979v9().b[75][1]++;
      }
    }
  }));
  /* istanbul ignore next */
  cov_bcnj979v9().s[300]++;
  visitor.visit(re);
  /* istanbul ignore next */
  cov_bcnj979v9().s[301]++;
  output += pattern.slice(lastEmittedIndex);
  /* istanbul ignore next */
  cov_bcnj979v9().s[302]++;
  return output;
}
export function fixNewline(pattern) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[61]++;
  cov_bcnj979v9().s[303]++;
  return pattern.replace(/\n/g, '\\r?\\n');
}
// brace expansion for ripgrep
/**
 * Split string given first opportunity for brace expansion in the string.
 * - If the brace is prepended by a \ character, then it is escaped.
 * - Does not process escapes that are within the sub-glob.
 * - If two unescaped `{` occur before `}`, then ripgrep will return an error for brace nesting, so don't split on those.
 */
function getEscapeAwareSplitStringForRipgrep(pattern) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[62]++;
  let inBraces =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[304]++, false);
  let escaped =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[305]++, false);
  let fixedStart =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[306]++, '');
  let strInBraces =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[307]++, '');
  /* istanbul ignore next */
  cov_bcnj979v9().s[308]++;
  for (let i =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[309]++, 0); i < pattern.length; i++) {
    const char =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[310]++, pattern[i]);
    /* istanbul ignore next */
    cov_bcnj979v9().s[311]++;
    switch (char) {
      case '\\':
        /* istanbul ignore next */
        cov_bcnj979v9().b[76][0]++;
        cov_bcnj979v9().s[312]++;
        if (escaped) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[77][0]++;
          cov_bcnj979v9().s[313]++;
          // If we're already escaped, then just leave the escaped slash and the preceeding slash that escapes it.
          // The two escaped slashes will result in a single slash and whatever processes the glob later will properly process the escape
          if (inBraces) {
            /* istanbul ignore next */
            cov_bcnj979v9().b[78][0]++;
            cov_bcnj979v9().s[314]++;
            strInBraces += '\\' + char;
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[78][1]++;
            cov_bcnj979v9().s[315]++;
            fixedStart += '\\' + char;
          }
          /* istanbul ignore next */
          cov_bcnj979v9().s[316]++;
          escaped = false;
        } else {
          /* istanbul ignore next */
          cov_bcnj979v9().b[77][1]++;
          cov_bcnj979v9().s[317]++;
          escaped = true;
        }
        /* istanbul ignore next */
        cov_bcnj979v9().s[318]++;
        break;
      case '{':
        /* istanbul ignore next */
        cov_bcnj979v9().b[76][1]++;
        cov_bcnj979v9().s[319]++;
        if (escaped) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[79][0]++;
          cov_bcnj979v9().s[320]++;
          // if we escaped this opening bracket, then it is to be taken literally. Remove the `\` because we've acknowleged it and add the `{` to the appropriate string
          if (inBraces) {
            /* istanbul ignore next */
            cov_bcnj979v9().b[80][0]++;
            cov_bcnj979v9().s[321]++;
            strInBraces += char;
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[80][1]++;
            cov_bcnj979v9().s[322]++;
            fixedStart += char;
          }
          /* istanbul ignore next */
          cov_bcnj979v9().s[323]++;
          escaped = false;
        } else {
          /* istanbul ignore next */
          cov_bcnj979v9().b[79][1]++;
          cov_bcnj979v9().s[324]++;
          if (inBraces) {
            /* istanbul ignore next */
            cov_bcnj979v9().b[81][0]++;
            cov_bcnj979v9().s[325]++;
            // ripgrep treats this as attempting to do a nested alternate group, which is invalid. Return with pattern including changes from escaped braces.
            return {
              strInBraces: fixedStart + '{' + strInBraces + '{' + pattern.substring(i + 1)
            };
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[81][1]++;
            cov_bcnj979v9().s[326]++;
            inBraces = true;
          }
        }
        /* istanbul ignore next */
        cov_bcnj979v9().s[327]++;
        break;
      case '}':
        /* istanbul ignore next */
        cov_bcnj979v9().b[76][2]++;
        cov_bcnj979v9().s[328]++;
        if (escaped) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[82][0]++;
          cov_bcnj979v9().s[329]++;
          // same as `}`, but for closing bracket
          if (inBraces) {
            /* istanbul ignore next */
            cov_bcnj979v9().b[83][0]++;
            cov_bcnj979v9().s[330]++;
            strInBraces += char;
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[83][1]++;
            cov_bcnj979v9().s[331]++;
            fixedStart += char;
          }
          /* istanbul ignore next */
          cov_bcnj979v9().s[332]++;
          escaped = false;
        } else {
          /* istanbul ignore next */
          cov_bcnj979v9().b[82][1]++;
          cov_bcnj979v9().s[333]++;
          if (inBraces) {
            /* istanbul ignore next */
            cov_bcnj979v9().b[84][0]++;
            cov_bcnj979v9().s[334]++;
            // we found an end bracket to a valid opening bracket. Return the appropriate strings.
            return {
              fixedStart,
              strInBraces,
              fixedEnd: pattern.substring(i + 1)
            };
          } else {
            /* istanbul ignore next */
            cov_bcnj979v9().b[84][1]++;
            cov_bcnj979v9().s[335]++;
            // if we're not in braces and not escaped, then this is a literal `}` character and we're still adding to fixedStart.
            fixedStart += char;
          }
        }
        /* istanbul ignore next */
        cov_bcnj979v9().s[336]++;
        break;
      default:
        /* istanbul ignore next */
        cov_bcnj979v9().b[76][3]++;
        cov_bcnj979v9().s[337]++;
        // similar to the `\\` case, we didn't do anything with the escape, so we should re-insert it into the appropriate string
        // to be consumed later when individual parts of the glob are processed
        if (inBraces) {
          /* istanbul ignore next */
          cov_bcnj979v9().b[85][0]++;
          cov_bcnj979v9().s[338]++;
          strInBraces += (escaped ?
          /* istanbul ignore next */
          (cov_bcnj979v9().b[86][0]++, '\\') :
          /* istanbul ignore next */
          (cov_bcnj979v9().b[86][1]++, '')) + char;
        } else {
          /* istanbul ignore next */
          cov_bcnj979v9().b[85][1]++;
          cov_bcnj979v9().s[339]++;
          fixedStart += (escaped ?
          /* istanbul ignore next */
          (cov_bcnj979v9().b[87][0]++, '\\') :
          /* istanbul ignore next */
          (cov_bcnj979v9().b[87][1]++, '')) + char;
        }
        /* istanbul ignore next */
        cov_bcnj979v9().s[340]++;
        escaped = false;
        /* istanbul ignore next */
        cov_bcnj979v9().s[341]++;
        break;
    }
  }
  // we are haven't hit the last brace, so no splitting should occur. Return with pattern including changes from escaped braces.
  /* istanbul ignore next */
  cov_bcnj979v9().s[342]++;
  return {
    strInBraces: fixedStart + (inBraces ?
    /* istanbul ignore next */
    (cov_bcnj979v9().b[88][0]++, '{' + strInBraces) :
    /* istanbul ignore next */
    (cov_bcnj979v9().b[88][1]++, ''))
  };
}
/**
 * Parses out curly braces and returns equivalent globs. Only supports one level of nesting.
 * Exported for testing.
 */
export function performBraceExpansionForRipgrep(pattern) {
  /* istanbul ignore next */
  cov_bcnj979v9().f[63]++;
  const {
    fixedStart,
    strInBraces,
    fixedEnd
  } =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[343]++, getEscapeAwareSplitStringForRipgrep(pattern));
  /* istanbul ignore next */
  cov_bcnj979v9().s[344]++;
  if (
  /* istanbul ignore next */
  (cov_bcnj979v9().b[90][0]++, fixedStart === undefined) ||
  /* istanbul ignore next */
  (cov_bcnj979v9().b[90][1]++, fixedEnd === undefined)) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[89][0]++;
    cov_bcnj979v9().s[345]++;
    return [strInBraces];
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[89][1]++;
  }
  let arr =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[346]++, splitGlobAware(strInBraces, ','));
  /* istanbul ignore next */
  cov_bcnj979v9().s[347]++;
  if (!arr.length) {
    /* istanbul ignore next */
    cov_bcnj979v9().b[91][0]++;
    cov_bcnj979v9().s[348]++;
    // occurs if the braces are empty.
    arr = [''];
  } else
  /* istanbul ignore next */
  {
    cov_bcnj979v9().b[91][1]++;
  }
  const ends =
  /* istanbul ignore next */
  (cov_bcnj979v9().s[349]++, performBraceExpansionForRipgrep(fixedEnd));
  /* istanbul ignore next */
  cov_bcnj979v9().s[350]++;
  return arr.flatMap(elem => {
    /* istanbul ignore next */
    cov_bcnj979v9().f[64]++;
    const start =
    /* istanbul ignore next */
    (cov_bcnj979v9().s[351]++, fixedStart + elem);
    /* istanbul ignore next */
    cov_bcnj979v9().s[352]++;
    return ends.map(end => {
      /* istanbul ignore next */
      cov_bcnj979v9().f[65]++;
      cov_bcnj979v9().s[353]++;
      return start + end;
    });
  });
}
