  onRevealRangeRequest(e) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[19]++;
    // Using the future viewport here in order to handle multiple
    // incoming reveal range requests that might all desire to be animated
    const desiredScrollTop =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[115]++, this._computeScrollTopToRevealRange(this._context.viewLayout.getFutureViewport(), e.source, e.minimalReveal, e.range, e.selections, e.verticalType));
    /* istanbul ignore next */
    cov_25t0it8blf().s[116]++;
    if (desiredScrollTop === -1) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[4][0]++;
      cov_25t0it8blf().s[117]++;
      // marker to abort the reveal range request
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[4][1]++;
    }
    // validate the new desired scroll top
    let newScrollPosition =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[118]++, this._context.viewLayout.validateScrollPosition({
      scrollTop: desiredScrollTop
    }));
    /* istanbul ignore next */
    cov_25t0it8blf().s[119]++;
    if (e.revealHorizontal) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[5][0]++;
      cov_25t0it8blf().s[120]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[7][0]++, e.range) &&
      /* istanbul ignore next */
      (cov_25t0it8blf().b[7][1]++, e.range.startLineNumber !== e.range.endLineNumber)) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[6][0]++;
        cov_25t0it8blf().s[121]++;
        // Two or more lines? => scroll to base (That's how you see most of the two lines)
        newScrollPosition = {
          scrollTop: newScrollPosition.scrollTop,
          scrollLeft: 0
        };
      } else {
        /* istanbul ignore next */
        cov_25t0it8blf().b[6][1]++;
        cov_25t0it8blf().s[122]++;
        if (e.range) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[8][0]++;
          cov_25t0it8blf().s[123]++;
          // We don't necessarily know the horizontal offset of this range since the line might not be in the view...
          this._horizontalRevealRequest = new HorizontalRevealRangeRequest(e.minimalReveal, e.range.startLineNumber, e.range.startColumn, e.range.endColumn, this._context.viewLayout.getCurrentScrollTop(), newScrollPosition.scrollTop, e.scrollType);
        } else {
          /* istanbul ignore next */
          cov_25t0it8blf().b[8][1]++;
          cov_25t0it8blf().s[124]++;
          if (
          /* istanbul ignore next */
          (cov_25t0it8blf().b[10][0]++, e.selections) &&
          /* istanbul ignore next */
          (cov_25t0it8blf().b[10][1]++, e.selections.length > 0)) {
            /* istanbul ignore next */
            cov_25t0it8blf().b[9][0]++;
            cov_25t0it8blf().s[125]++;
            this._horizontalRevealRequest = new HorizontalRevealSelectionsRequest(e.minimalReveal, e.selections, this._context.viewLayout.getCurrentScrollTop(), newScrollPosition.scrollTop, e.scrollType);
          } else
          /* istanbul ignore next */
          {
            cov_25t0it8blf().b[9][1]++;
          }
        }
      }
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[5][1]++;
      cov_25t0it8blf().s[126]++;
      this._horizontalRevealRequest = null;
    }
    const scrollTopDelta =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[127]++, Math.abs(this._context.viewLayout.getCurrentScrollTop() - newScrollPosition.scrollTop));
    const scrollType =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[128]++, scrollTopDelta <= this._lineHeight ?
    /* istanbul ignore next */
    (cov_25t0it8blf().b[11][0]++, 1 /* ScrollType.Immediate */) :
    /* istanbul ignore next */
    (cov_25t0it8blf().b[11][1]++, e.scrollType));
    /* istanbul ignore next */
    cov_25t0it8blf().s[129]++;
    this._context.viewModel.viewLayout.setScrollPosition(newScrollPosition, scrollType);
    /* istanbul ignore next */
    cov_25t0it8blf().s[130]++;
    return true;
