  linesVisibleRangesForRange(_range, includeNewLines) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[28]++;
    cov_25t0it8blf().s[184]++;
    if (this.shouldRender()) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[32][0]++;
      cov_25t0it8blf().s[185]++;
      // Cannot read from the DOM because it is dirty
      // i.e. the model & the dom are out of sync, so I'd be reading something stale
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[32][1]++;
    }
    const originalEndLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[186]++, _range.endLineNumber);
    const range =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[187]++, Range.intersectRanges(_range, this._lastRenderedData.getCurrentVisibleRange()));
    /* istanbul ignore next */
    cov_25t0it8blf().s[188]++;
    if (!range) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[33][0]++;
      cov_25t0it8blf().s[189]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[33][1]++;
    }
    const visibleRanges =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[190]++, []);
    let visibleRangesLen =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[191]++, 0);
    const domReadingContext =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[192]++, new DomReadingContext(this.domNode.domNode, this._textRangeRestingSpot));
    let nextLineModelLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[193]++, 0);
    /* istanbul ignore next */
    cov_25t0it8blf().s[194]++;
    if (includeNewLines) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[34][0]++;
      cov_25t0it8blf().s[195]++;
      nextLineModelLineNumber = this._context.viewModel.coordinatesConverter.convertViewPositionToModelPosition(new Position(range.startLineNumber, 1)).lineNumber;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[34][1]++;
    }
    const rendStartLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[196]++, this._visibleLines.getStartLineNumber());
    const rendEndLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[197]++, this._visibleLines.getEndLineNumber());
    /* istanbul ignore next */
    cov_25t0it8blf().s[198]++;
    for (let lineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[199]++, range.startLineNumber); lineNumber <= range.endLineNumber; lineNumber++) {
      /* istanbul ignore next */
      cov_25t0it8blf().s[200]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[36][0]++, lineNumber < rendStartLineNumber) ||
      /* istanbul ignore next */
      (cov_25t0it8blf().b[36][1]++, lineNumber > rendEndLineNumber)) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[35][0]++;
        cov_25t0it8blf().s[201]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[35][1]++;
      }
      const startColumn =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[202]++, lineNumber === range.startLineNumber ?
      /* istanbul ignore next */
      (cov_25t0it8blf().b[37][0]++, range.startColumn) :
      /* istanbul ignore next */
      (cov_25t0it8blf().b[37][1]++, 1));
      const continuesInNextLine =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[203]++, lineNumber !== originalEndLineNumber);
      const endColumn =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[204]++, continuesInNextLine ?
      /* istanbul ignore next */
      (cov_25t0it8blf().b[38][0]++, this._context.viewModel.getLineMaxColumn(lineNumber)) :
      /* istanbul ignore next */
      (cov_25t0it8blf().b[38][1]++, range.endColumn));
      const visibleLine =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[205]++, this._visibleLines.getVisibleLine(lineNumber));
      const visibleRangesForLine =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[206]++, visibleLine.getVisibleRangesForRange(lineNumber, startColumn, endColumn, domReadingContext));
      /* istanbul ignore next */
      cov_25t0it8blf().s[207]++;
      if (!visibleRangesForLine) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[39][0]++;
        cov_25t0it8blf().s[208]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[39][1]++;
      }
      cov_25t0it8blf().s[209]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[41][0]++, includeNewLines) &&
      /* istanbul ignore next */
      (cov_25t0it8blf().b[41][1]++, lineNumber < originalEndLineNumber)) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[40][0]++;
        const currentLineModelLineNumber =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[210]++, nextLineModelLineNumber);
        /* istanbul ignore next */
        cov_25t0it8blf().s[211]++;
        nextLineModelLineNumber = this._context.viewModel.coordinatesConverter.convertViewPositionToModelPosition(new Position(lineNumber + 1, 1)).lineNumber;
        /* istanbul ignore next */
        cov_25t0it8blf().s[212]++;
        if (currentLineModelLineNumber !== nextLineModelLineNumber) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[42][0]++;
          const floatHorizontalRange =
          /* istanbul ignore next */
          (cov_25t0it8blf().s[213]++, visibleRangesForLine.ranges[visibleRangesForLine.ranges.length - 1]);
          /* istanbul ignore next */
          cov_25t0it8blf().s[214]++;
          floatHorizontalRange.width += this._typicalHalfwidthCharacterWidth;
          /* istanbul ignore next */
          cov_25t0it8blf().s[215]++;
          if (this._context.viewModel.getTextDirection(currentLineModelLineNumber) === TextDirection.RTL) {
            /* istanbul ignore next */
            cov_25t0it8blf().b[43][0]++;
            cov_25t0it8blf().s[216]++;
            floatHorizontalRange.left -= this._typicalHalfwidthCharacterWidth;
          } else
          /* istanbul ignore next */
          {
            cov_25t0it8blf().b[43][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[42][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[40][1]++;
      }
      cov_25t0it8blf().s[217]++;
      visibleRanges[visibleRangesLen++] = new LineVisibleRanges(visibleRangesForLine.outsideRenderedLine, lineNumber, HorizontalRange.from(visibleRangesForLine.ranges), continuesInNextLine);
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[218]++;
    this._updateLineWidthsSlowIfDomDidLayout(domReadingContext);
    /* istanbul ignore next */
    cov_25t0it8blf().s[219]++;
    if (visibleRangesLen === 0) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[44][0]++;
      cov_25t0it8blf().s[220]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[44][1]++;
    }
    cov_25t0it8blf().s[221]++;
