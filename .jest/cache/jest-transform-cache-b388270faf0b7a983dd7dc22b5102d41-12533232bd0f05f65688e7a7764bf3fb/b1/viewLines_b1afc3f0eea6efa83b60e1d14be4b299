  }
  _visibleRangesForLineRange(lineNumber, startColumn, endColumn) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[29]++;
    cov_25t0it8blf().s[222]++;
    if (this.shouldRender()) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[45][0]++;
      cov_25t0it8blf().s[223]++;
      // Cannot read from the DOM because it is dirty
      // i.e. the model & the dom are out of sync, so I'd be reading something stale
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[45][1]++;
    }
    cov_25t0it8blf().s[224]++;
    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[47][0]++, lineNumber < this._visibleLines.getStartLineNumber()) ||
    /* istanbul ignore next */
    (cov_25t0it8blf().b[47][1]++, lineNumber > this._visibleLines.getEndLineNumber())) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[46][0]++;
      cov_25t0it8blf().s[225]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[46][1]++;
    }
    const domReadingContext =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[226]++, new DomReadingContext(this.domNode.domNode, this._textRangeRestingSpot));
    const result =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[227]++, this._visibleLines.getVisibleLine(lineNumber).getVisibleRangesForRange(lineNumber, startColumn, endColumn, domReadingContext));
    /* istanbul ignore next */
    cov_25t0it8blf().s[228]++;
    this._updateLineWidthsSlowIfDomDidLayout(domReadingContext);
    /* istanbul ignore next */
    cov_25t0it8blf().s[229]++;
    return result;
  }
  _lineIsRenderedRTL(lineNumber) {
    /* istanbul ignore next */
