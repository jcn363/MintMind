    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[22][0]++, lineNumber < 1) ||
    /* istanbul ignore next */
    (cov_25t0it8blf().b[22][1]++, lineNumber > this._context.viewModel.getLineCount())) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[21][0]++;
      cov_25t0it8blf().s[151]++;
      // lineNumber is outside range
