    }
    cov_25t0it8blf().s[237]++;
    return new HorizontalPosition(visibleRanges.outsideRenderedLine, visibleRanges.ranges[0].left);
  }
  // --- implementation
  updateLineWidths() {
    /* istanbul ignore next */
    cov_25t0it8blf().f[32]++;
    cov_25t0it8blf().s[238]++;
    this._updateLineWidths(false);
  }
  /**
   * Updates the max line width if it is fast to compute.
   * Returns true if all lines were taken into account.
   * Returns false if some lines need to be reevaluated (in a slow fashion).
   */
  _updateLineWidthsFast() {
    /* istanbul ignore next */
    cov_25t0it8blf().f[33]++;
    cov_25t0it8blf().s[239]++;
    return this._updateLineWidths(true);
  }
  _updateLineWidthsSlow() {
    /* istanbul ignore next */
    cov_25t0it8blf().f[34]++;
    cov_25t0it8blf().s[240]++;
    this._updateLineWidths(false);
  }
  /**
   * Update the line widths using DOM layout information after someone else
   * has caused a synchronous layout.
   */
  _updateLineWidthsSlowIfDomDidLayout(domReadingContext) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[35]++;
    cov_25t0it8blf().s[241]++;
    if (!domReadingContext.didDomLayout) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[51][0]++;
      cov_25t0it8blf().s[242]++;
      // only proceed if we just did a layout
      return;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[51][1]++;
    }
    cov_25t0it8blf().s[243]++;
    if (!this._asyncUpdateLineWidths.isScheduled()) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[52][0]++;
      cov_25t0it8blf().s[244]++;
      // reading widths is not scheduled => widths are up-to-date
      return;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[52][1]++;
    }
    cov_25t0it8blf().s[245]++;
    this._asyncUpdateLineWidths.cancel();
    /* istanbul ignore next */
    cov_25t0it8blf().s[246]++;
    this._updateLineWidthsSlow();
  }
  _updateLineWidths(fast) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[36]++;
    const rendStartLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[247]++, this._visibleLines.getStartLineNumber());
    const rendEndLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[248]++, this._visibleLines.getEndLineNumber());
    let localMaxLineWidth =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[249]++, 1);
    let allWidthsComputed =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[250]++, true);
    /* istanbul ignore next */
    cov_25t0it8blf().s[251]++;
    for (let lineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[252]++, rendStartLineNumber); lineNumber <= rendEndLineNumber; lineNumber++) {
      const visibleLine =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[253]++, this._visibleLines.getVisibleLine(lineNumber));
      /* istanbul ignore next */
      cov_25t0it8blf().s[254]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[54][0]++, fast) &&
      /* istanbul ignore next */
      (cov_25t0it8blf().b[54][1]++, !visibleLine.getWidthIsFast())) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[53][0]++;
        cov_25t0it8blf().s[255]++;
        // Cannot compute width in a fast way for this line
        allWidthsComputed = false;
        /* istanbul ignore next */
        cov_25t0it8blf().s[256]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[53][1]++;
      }
      cov_25t0it8blf().s[257]++;
      localMaxLineWidth = Math.max(localMaxLineWidth, visibleLine.getWidth(null));
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[258]++;
    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[56][0]++, allWidthsComputed) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[56][1]++, rendStartLineNumber === 1) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[56][2]++, rendEndLineNumber === this._context.viewModel.getLineCount())) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[55][0]++;
      cov_25t0it8blf().s[259]++;
      // we know the max line width for all the lines
      this._maxLineWidth = 0;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[55][1]++;
    }
    cov_25t0it8blf().s[260]++;
    this._ensureMaxLineWidth(localMaxLineWidth);
    /* istanbul ignore next */
    cov_25t0it8blf().s[261]++;
    return allWidthsComputed;
  }
  _checkMonospaceFontAssumptions() {
    /* istanbul ignore next */
    cov_25t0it8blf().f[37]++;
    // Problems with monospace assumptions are more apparent for longer lines,
    // as small rounding errors start to sum up, so we will select the longest
    // line for a closer inspection
    let longestLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[262]++, -1);
    let longestWidth =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[263]++, -1);
    const rendStartLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[264]++, this._visibleLines.getStartLineNumber());
    const rendEndLineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[265]++, this._visibleLines.getEndLineNumber());
    /* istanbul ignore next */
    cov_25t0it8blf().s[266]++;
    for (let lineNumber =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[267]++, rendStartLineNumber); lineNumber <= rendEndLineNumber; lineNumber++) {
      const visibleLine =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[268]++, this._visibleLines.getVisibleLine(lineNumber));
      /* istanbul ignore next */
      cov_25t0it8blf().s[269]++;
      if (visibleLine.needsMonospaceFontCheck()) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[57][0]++;
        const lineWidth =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[270]++, visibleLine.getWidth(null));
        /* istanbul ignore next */
        cov_25t0it8blf().s[271]++;
        if (lineWidth > longestWidth) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[58][0]++;
          cov_25t0it8blf().s[272]++;
          longestWidth = lineWidth;
          /* istanbul ignore next */
          cov_25t0it8blf().s[273]++;
          longestLineNumber = lineNumber;
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[58][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[57][1]++;
      }
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[274]++;
    if (longestLineNumber === -1) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[59][0]++;
      cov_25t0it8blf().s[275]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[59][1]++;
    }
    cov_25t0it8blf().s[276]++;
    if (!this._visibleLines.getVisibleLine(longestLineNumber).monospaceAssumptionsAreValid()) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[60][0]++;
      cov_25t0it8blf().s[277]++;
      for (let lineNumber =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[278]++, rendStartLineNumber); lineNumber <= rendEndLineNumber; lineNumber++) {
        const visibleLine =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[279]++, this._visibleLines.getVisibleLine(lineNumber));
        /* istanbul ignore next */
        cov_25t0it8blf().s[280]++;
        visibleLine.onMonospaceAssumptionsInvalidated();
      }
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[60][1]++;
    }
  }
  prepareRender() {
    /* istanbul ignore next */
    cov_25t0it8blf().f[38]++;
    cov_25t0it8blf().s[281]++;
    throw new Error('Not supported');
  }
  render() {
    /* istanbul ignore next */
    cov_25t0it8blf().f[39]++;
    cov_25t0it8blf().s[282]++;
    throw new Error('Not supported');
  }
  renderText(viewportData) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[40]++;
    cov_25t0it8blf().s[283]++;
    // (1) render lines - ensures lines are in the DOM
    this._visibleLines.renderLines(viewportData);
    /* istanbul ignore next */
    cov_25t0it8blf().s[284]++;
    this._lastRenderedData.setCurrentVisibleRange(viewportData.visibleRange);
    /* istanbul ignore next */
    cov_25t0it8blf().s[285]++;
    this.domNode.setWidth(this._context.viewLayout.getScrollWidth());
    /* istanbul ignore next */
    cov_25t0it8blf().s[286]++;
    this.domNode.setHeight(Math.min(this._context.viewLayout.getScrollHeight(), 1000000));
    // (2) compute horizontal scroll position:
    //  - this must happen after the lines are in the DOM since it might need a line that rendered just now
    //  - it might change `scrollWidth` and `scrollLeft`
    /* istanbul ignore next */
    cov_25t0it8blf().s[287]++;
    if (this._horizontalRevealRequest) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[61][0]++;
      const horizontalRevealRequest =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[288]++, this._horizontalRevealRequest);
      // Check that we have the line that contains the horizontal range in the viewport
      /* istanbul ignore next */
      cov_25t0it8blf().s[289]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[63][0]++, viewportData.startLineNumber <= horizontalRevealRequest.minLineNumber) &&
      /* istanbul ignore next */
      (cov_25t0it8blf().b[63][1]++, horizontalRevealRequest.maxLineNumber <= viewportData.endLineNumber)) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[62][0]++;
        cov_25t0it8blf().s[290]++;
        this._horizontalRevealRequest = null;
        // allow `visibleRangesForRange2` to work
        /* istanbul ignore next */
        cov_25t0it8blf().s[291]++;
        this.onDidRender();
        // compute new scroll position
        const newScrollLeft =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[292]++, this._computeScrollLeftToReveal(horizontalRevealRequest));
        /* istanbul ignore next */
        cov_25t0it8blf().s[293]++;
        if (newScrollLeft) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[64][0]++;
          cov_25t0it8blf().s[294]++;
          if (
          /* istanbul ignore next */
          (cov_25t0it8blf().b[66][0]++, !this._isViewportWrapping) &&
          /* istanbul ignore next */
          (cov_25t0it8blf().b[66][1]++, !newScrollLeft.hasRTL)) {
            /* istanbul ignore next */
            cov_25t0it8blf().b[65][0]++;
            cov_25t0it8blf().s[295]++;
            // ensure `scrollWidth` is large enough
            this._ensureMaxLineWidth(newScrollLeft.maxHorizontalOffset);
          } else
          /* istanbul ignore next */
          {
            cov_25t0it8blf().b[65][1]++;
          }
          // set `scrollLeft`
          cov_25t0it8blf().s[296]++;
          this._context.viewModel.viewLayout.setScrollPosition({
            scrollLeft: newScrollLeft.scrollLeft
          }, horizontalRevealRequest.scrollType);
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[64][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[62][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[61][1]++;
    }
    // Update max line width (not so important, it is just so the horizontal scrollbar doesn't get too small)
    cov_25t0it8blf().s[297]++;
    if (!this._updateLineWidthsFast()) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[67][0]++;
      cov_25t0it8blf().s[298]++;
      // Computing the width of some lines would be slow => delay it
      this._asyncUpdateLineWidths.schedule();
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[67][1]++;
      cov_25t0it8blf().s[299]++;
      this._asyncUpdateLineWidths.cancel();
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[300]++;
    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[69][0]++, platform.isLinux) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[69][1]++, !this._asyncCheckMonospaceFontAssumptions.isScheduled())) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[68][0]++;
      const rendStartLineNumber =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[301]++, this._visibleLines.getStartLineNumber());
      const rendEndLineNumber =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[302]++, this._visibleLines.getEndLineNumber());
      /* istanbul ignore next */
      cov_25t0it8blf().s[303]++;
      for (let lineNumber =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[304]++, rendStartLineNumber); lineNumber <= rendEndLineNumber; lineNumber++) {
        const visibleLine =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[305]++, this._visibleLines.getVisibleLine(lineNumber));
        /* istanbul ignore next */
        cov_25t0it8blf().s[306]++;
        if (visibleLine.needsMonospaceFontCheck()) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[70][0]++;
          cov_25t0it8blf().s[307]++;
          this._asyncCheckMonospaceFontAssumptions.schedule();
          /* istanbul ignore next */
          cov_25t0it8blf().s[308]++;
          break;
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[70][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[68][1]++;
    }
    // (3) handle scrolling
    cov_25t0it8blf().s[309]++;
    this._linesContent.setLayerHinting(this._canUseLayerHinting);
    /* istanbul ignore next */
    cov_25t0it8blf().s[310]++;
    this._linesContent.setContain('strict');
    const adjustedScrollTop =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[311]++, this._context.viewLayout.getCurrentScrollTop() - viewportData.bigNumbersDelta);
    /* istanbul ignore next */
    cov_25t0it8blf().s[312]++;
    this._linesContent.setTop(-adjustedScrollTop);
    /* istanbul ignore next */
    cov_25t0it8blf().s[313]++;
    this._linesContent.setLeft(-this._context.viewLayout.getCurrentScrollLeft());
  }
  // --- width
  _ensureMaxLineWidth(lineWidth) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[41]++;
    const iLineWidth =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[314]++, Math.ceil(lineWidth));
    /* istanbul ignore next */
    cov_25t0it8blf().s[315]++;
    if (this._maxLineWidth < iLineWidth) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[71][0]++;
      cov_25t0it8blf().s[316]++;
      this._maxLineWidth = iLineWidth;
      /* istanbul ignore next */
      cov_25t0it8blf().s[317]++;
      this._context.viewModel.viewLayout.setMaxLineWidth(this._maxLineWidth);
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[71][1]++;
    }
  }
  _computeScrollTopToRevealRange(viewport, source, minimalReveal, range, selections, verticalType) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[42]++;
    const viewportStartY =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[318]++, viewport.top);
    const viewportHeight =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[319]++, viewport.height);
    const viewportEndY =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[320]++, viewportStartY + viewportHeight);
    let boxIsSingleRange;
    let boxStartY;
    let boxEndY;
    /* istanbul ignore next */
    cov_25t0it8blf().s[321]++;
    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[73][0]++, selections) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[73][1]++, selections.length > 0)) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[72][0]++;
      let minLineNumber =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[322]++, selections[0].startLineNumber);
      let maxLineNumber =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[323]++, selections[0].endLineNumber);
      /* istanbul ignore next */
      cov_25t0it8blf().s[324]++;
      for (let i =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[325]++, 1), len =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[326]++, selections.length); i < len; i++) {
        const selection =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[327]++, selections[i]);
        /* istanbul ignore next */
        cov_25t0it8blf().s[328]++;
        minLineNumber = Math.min(minLineNumber, selection.startLineNumber);
        /* istanbul ignore next */
        cov_25t0it8blf().s[329]++;
        maxLineNumber = Math.max(maxLineNumber, selection.endLineNumber);
      }
      /* istanbul ignore next */
      cov_25t0it8blf().s[330]++;
      boxIsSingleRange = false;
      /* istanbul ignore next */
      cov_25t0it8blf().s[331]++;
      boxStartY = this._context.viewLayout.getVerticalOffsetForLineNumber(minLineNumber);
      /* istanbul ignore next */
      cov_25t0it8blf().s[332]++;
      boxEndY = this._context.viewLayout.getVerticalOffsetForLineNumber(maxLineNumber) + this._lineHeight;
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[72][1]++;
      cov_25t0it8blf().s[333]++;
      if (range) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[74][0]++;
        cov_25t0it8blf().s[334]++;
        boxIsSingleRange = true;
        /* istanbul ignore next */
        cov_25t0it8blf().s[335]++;
        boxStartY = this._context.viewLayout.getVerticalOffsetForLineNumber(range.startLineNumber);
        /* istanbul ignore next */
        cov_25t0it8blf().s[336]++;
        boxEndY = this._context.viewLayout.getVerticalOffsetForLineNumber(range.endLineNumber) + this._lineHeight;
      } else {
        /* istanbul ignore next */
        cov_25t0it8blf().b[74][1]++;
        cov_25t0it8blf().s[337]++;
        return -1;
      }
    }
    const shouldIgnoreScrollOff =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[338]++, (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[75][0]++, source === 'mouse') ||
    /* istanbul ignore next */
    (cov_25t0it8blf().b[75][1]++, minimalReveal)) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[75][2]++, this._cursorSurroundingLinesStyle === 'default'));
    let paddingTop =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[339]++, 0);
    let paddingBottom =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[340]++, 0);
    /* istanbul ignore next */
    cov_25t0it8blf().s[341]++;
    if (!shouldIgnoreScrollOff) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[76][0]++;
      const maxLinesInViewport =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[342]++, viewportHeight / this._lineHeight);
      const surroundingLines =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[343]++, Math.max(this._cursorSurroundingLines, this._stickyScrollEnabled ?
      /* istanbul ignore next */
      (cov_25t0it8blf().b[77][0]++, this._maxNumberStickyLines) :
      /* istanbul ignore next */
      (cov_25t0it8blf().b[77][1]++, 0)));
      const context =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[344]++, Math.min(maxLinesInViewport / 2, surroundingLines));
      /* istanbul ignore next */
      cov_25t0it8blf().s[345]++;
      paddingTop = context * this._lineHeight;
      /* istanbul ignore next */
      cov_25t0it8blf().s[346]++;
      paddingBottom = Math.max(0, context - 1) * this._lineHeight;
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[76][1]++;
      cov_25t0it8blf().s[347]++;
      if (!minimalReveal) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[78][0]++;
        cov_25t0it8blf().s[348]++;
        // Reveal one more line above (this case is hit when dragging)
        paddingTop = this._lineHeight;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[78][1]++;
      }
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[349]++;
    if (!minimalReveal) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[79][0]++;
      cov_25t0it8blf().s[350]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[81][0]++, verticalType === 0 /* viewEvents.VerticalRevealType.Simple */) ||
      /* istanbul ignore next */
      (cov_25t0it8blf().b[81][1]++, verticalType === 4) /* viewEvents.VerticalRevealType.Bottom */) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[80][0]++;
        cov_25t0it8blf().s[351]++;
        // Reveal one line more when the last line would be covered by the scrollbar - arrow down case or revealing a line explicitly at bottom
        paddingBottom += this._lineHeight;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[80][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[79][1]++;
    }
    cov_25t0it8blf().s[352]++;
    boxStartY -= paddingTop;
    /* istanbul ignore next */
    cov_25t0it8blf().s[353]++;
    boxEndY += paddingBottom;
    let newScrollTop;
    /* istanbul ignore next */
    cov_25t0it8blf().s[354]++;
    if (boxEndY - boxStartY > viewportHeight) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[82][0]++;
      cov_25t0it8blf().s[355]++;
      // the box is larger than the viewport ... scroll to its top
      if (!boxIsSingleRange) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[83][0]++;
        cov_25t0it8blf().s[356]++;
        // do not reveal multiple cursors if there are more than fit the viewport
        return -1;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[83][1]++;
      }
      cov_25t0it8blf().s[357]++;
      newScrollTop = boxStartY;
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[82][1]++;
      cov_25t0it8blf().s[358]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[85][0]++, verticalType === 5 /* viewEvents.VerticalRevealType.NearTop */) ||
      /* istanbul ignore next */
      (cov_25t0it8blf().b[85][1]++, verticalType === 6) /* viewEvents.VerticalRevealType.NearTopIfOutsideViewport */) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[84][0]++;
        cov_25t0it8blf().s[359]++;
        if (
        /* istanbul ignore next */
        (cov_25t0it8blf().b[87][0]++, verticalType === 6 /* viewEvents.VerticalRevealType.NearTopIfOutsideViewport */) &&
        /* istanbul ignore next */
        (cov_25t0it8blf().b[87][1]++, viewportStartY <= boxStartY) &&
        /* istanbul ignore next */
        (cov_25t0it8blf().b[87][2]++, boxEndY <= viewportEndY)) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[86][0]++;
          cov_25t0it8blf().s[360]++;
          // Box is already in the viewport... do nothing
          newScrollTop = viewportStartY;
        } else {
          /* istanbul ignore next */
          cov_25t0it8blf().b[86][1]++;
          // We want a gap that is 20% of the viewport, but with a minimum of 5 lines
          const desiredGapAbove =
          /* istanbul ignore next */
          (cov_25t0it8blf().s[361]++, Math.max(5 * this._lineHeight, viewportHeight * 0.2));
          // Try to scroll just above the box with the desired gap
          const desiredScrollTop =
          /* istanbul ignore next */
          (cov_25t0it8blf().s[362]++, boxStartY - desiredGapAbove);
          // But ensure that the box is not pushed out of viewport
          const minScrollTop =
          /* istanbul ignore next */
          (cov_25t0it8blf().s[363]++, boxEndY - viewportHeight);
          /* istanbul ignore next */
          cov_25t0it8blf().s[364]++;
          newScrollTop = Math.max(minScrollTop, desiredScrollTop);
        }
      } else {
        /* istanbul ignore next */
        cov_25t0it8blf().b[84][1]++;
        cov_25t0it8blf().s[365]++;
        if (
        /* istanbul ignore next */
        (cov_25t0it8blf().b[89][0]++, verticalType === 1 /* viewEvents.VerticalRevealType.Center */) ||
        /* istanbul ignore next */
        (cov_25t0it8blf().b[89][1]++, verticalType === 2) /* viewEvents.VerticalRevealType.CenterIfOutsideViewport */) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[88][0]++;
          cov_25t0it8blf().s[366]++;
          if (
          /* istanbul ignore next */
          (cov_25t0it8blf().b[91][0]++, verticalType === 2 /* viewEvents.VerticalRevealType.CenterIfOutsideViewport */) &&
          /* istanbul ignore next */
          (cov_25t0it8blf().b[91][1]++, viewportStartY <= boxStartY) &&
          /* istanbul ignore next */
          (cov_25t0it8blf().b[91][2]++, boxEndY <= viewportEndY)) {
            /* istanbul ignore next */
            cov_25t0it8blf().b[90][0]++;
            cov_25t0it8blf().s[367]++;
            // Box is already in the viewport... do nothing
            newScrollTop = viewportStartY;
          } else {
            /* istanbul ignore next */
            cov_25t0it8blf().b[90][1]++;
            // Box is outside the viewport... center it
            const boxMiddleY =
            /* istanbul ignore next */
            (cov_25t0it8blf().s[368]++, (boxStartY + boxEndY) / 2);
            /* istanbul ignore next */
            cov_25t0it8blf().s[369]++;
            newScrollTop = Math.max(0, boxMiddleY - viewportHeight / 2);
          }
        } else {
          /* istanbul ignore next */
          cov_25t0it8blf().b[88][1]++;
          cov_25t0it8blf().s[370]++;
          newScrollTop = this._computeMinimumScrolling(viewportStartY, viewportEndY, boxStartY, boxEndY, verticalType === 3 /* viewEvents.VerticalRevealType.Top */, verticalType === 4 /* viewEvents.VerticalRevealType.Bottom */);
        }
      }
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[371]++;
    return newScrollTop;
  }
  _computeScrollLeftToReveal(horizontalRevealRequest) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[43]++;
    const viewport =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[372]++, this._context.viewLayout.getCurrentViewport());
    const layoutInfo =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[373]++, this._context.configuration.options.get(165 /* EditorOption.layoutInfo */));
    const viewportStartX =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[374]++, viewport.left);
    const viewportEndX =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[375]++, viewportStartX + viewport.width - layoutInfo.verticalScrollbarWidth);
    let boxStartX =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[376]++, 1073741824) /* Constants.MAX_SAFE_SMALL_INTEGER */;
    let boxEndX =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[377]++, 0);
    let hasRTL =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[378]++, false);
    /* istanbul ignore next */
    cov_25t0it8blf().s[379]++;
    if (horizontalRevealRequest.type === 'range') {
      /* istanbul ignore next */
      cov_25t0it8blf().b[92][0]++;
      cov_25t0it8blf().s[380]++;
      hasRTL = this._lineIsRenderedRTL(horizontalRevealRequest.lineNumber);
      const visibleRanges =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[381]++, this._visibleRangesForLineRange(horizontalRevealRequest.lineNumber, horizontalRevealRequest.startColumn, horizontalRevealRequest.endColumn));
      /* istanbul ignore next */
      cov_25t0it8blf().s[382]++;
      if (!visibleRanges) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[93][0]++;
        cov_25t0it8blf().s[383]++;
        return null;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[93][1]++;
      }
      cov_25t0it8blf().s[384]++;
      for (const visibleRange of visibleRanges.ranges) {
        /* istanbul ignore next */
        cov_25t0it8blf().s[385]++;
        boxStartX = Math.min(boxStartX, Math.round(visibleRange.left));
        /* istanbul ignore next */
        cov_25t0it8blf().s[386]++;
        boxEndX = Math.max(boxEndX, Math.round(visibleRange.left + visibleRange.width));
      }
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[92][1]++;
      cov_25t0it8blf().s[387]++;
      for (const selection of horizontalRevealRequest.selections) {
        /* istanbul ignore next */
        cov_25t0it8blf().s[388]++;
        if (selection.startLineNumber !== selection.endLineNumber) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[94][0]++;
          cov_25t0it8blf().s[389]++;
          return null;
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[94][1]++;
        }
        const visibleRanges =
        /* istanbul ignore next */
        (cov_25t0it8blf().s[390]++, this._visibleRangesForLineRange(selection.startLineNumber, selection.startColumn, selection.endColumn));
        /* istanbul ignore next */
        cov_25t0it8blf().s[391]++;
        hasRTL ||= this._lineIsRenderedRTL(selection.startLineNumber);
        /* istanbul ignore next */
        cov_25t0it8blf().s[392]++;
        if (!visibleRanges) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[95][0]++;
          cov_25t0it8blf().s[393]++;
          return null;
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[95][1]++;
        }
        cov_25t0it8blf().s[394]++;
        for (const visibleRange of visibleRanges.ranges) {
          /* istanbul ignore next */
          cov_25t0it8blf().s[395]++;
          boxStartX = Math.min(boxStartX, Math.round(visibleRange.left));
          /* istanbul ignore next */
          cov_25t0it8blf().s[396]++;
          boxEndX = Math.max(boxEndX, Math.round(visibleRange.left + visibleRange.width));
        }
      }
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[397]++;
    if (!horizontalRevealRequest.minimalReveal) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[96][0]++;
      cov_25t0it8blf().s[398]++;
      boxStartX = Math.max(0, boxStartX - ViewLines.HORIZONTAL_EXTRA_PX);
      /* istanbul ignore next */
      cov_25t0it8blf().s[399]++;
      boxEndX += this._revealHorizontalRightPadding;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[96][1]++;
    }
    cov_25t0it8blf().s[400]++;
    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[98][0]++, horizontalRevealRequest.type === 'selections') &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[98][1]++, boxEndX - boxStartX > viewport.width)) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[97][0]++;
      cov_25t0it8blf().s[401]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[97][1]++;
    }
    const newScrollLeft =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[402]++, this._computeMinimumScrolling(viewportStartX, viewportEndX, boxStartX, boxEndX));
    /* istanbul ignore next */
    cov_25t0it8blf().s[403]++;
    return {
      scrollLeft: newScrollLeft,
      maxHorizontalOffset: boxEndX,
      hasRTL
    };
  }
  _computeMinimumScrolling(viewportStart, viewportEnd, boxStart, boxEnd, revealAtStart, revealAtEnd) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[44]++;
    cov_25t0it8blf().s[404]++;
    viewportStart = viewportStart | 0;
    /* istanbul ignore next */
    cov_25t0it8blf().s[405]++;
    viewportEnd = viewportEnd | 0;
    /* istanbul ignore next */
    cov_25t0it8blf().s[406]++;
    boxStart = boxStart | 0;
    /* istanbul ignore next */
    cov_25t0it8blf().s[407]++;
    boxEnd = boxEnd | 0;
    /* istanbul ignore next */
    cov_25t0it8blf().s[408]++;
    revealAtStart = !!revealAtStart;
    /* istanbul ignore next */
    cov_25t0it8blf().s[409]++;
    revealAtEnd = !!revealAtEnd;
    const viewportLength =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[410]++, viewportEnd - viewportStart);
    const boxLength =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[411]++, boxEnd - boxStart);
    /* istanbul ignore next */
    cov_25t0it8blf().s[412]++;
    if (boxLength < viewportLength) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[99][0]++;
      cov_25t0it8blf().s[413]++;
      // The box would fit in the viewport
      if (revealAtStart) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[100][0]++;
        cov_25t0it8blf().s[414]++;
        return boxStart;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[100][1]++;
      }
      cov_25t0it8blf().s[415]++;
      if (revealAtEnd) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[101][0]++;
        cov_25t0it8blf().s[416]++;
        return Math.max(0, boxEnd - viewportLength);
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[101][1]++;
      }
      cov_25t0it8blf().s[417]++;
      if (boxStart < viewportStart) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[102][0]++;
        cov_25t0it8blf().s[418]++;
        // The box is above the viewport
        return boxStart;
      } else {
        /* istanbul ignore next */
        cov_25t0it8blf().b[102][1]++;
        cov_25t0it8blf().s[419]++;
        if (boxEnd > viewportEnd) {
          /* istanbul ignore next */
          cov_25t0it8blf().b[103][0]++;
          cov_25t0it8blf().s[420]++;
          // The box is below the viewport
          return Math.max(0, boxEnd - viewportLength);
        } else
        /* istanbul ignore next */
        {
          cov_25t0it8blf().b[103][1]++;
        }
      }
    } else {
      /* istanbul ignore next */
      cov_25t0it8blf().b[99][1]++;
      cov_25t0it8blf().s[421]++;
      // The box would not fit in the viewport
      // Reveal the beginning of the box
      return boxStart;
    }
    /* istanbul ignore next */
    cov_25t0it8blf().s[422]++;
    return viewportStart;
  }
}
