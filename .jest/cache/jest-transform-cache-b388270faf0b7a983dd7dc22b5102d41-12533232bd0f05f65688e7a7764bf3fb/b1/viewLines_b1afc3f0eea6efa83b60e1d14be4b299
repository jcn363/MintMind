    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[48][1]++;
    }
    const visibleLine =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[232]++, this._visibleLines.getVisibleLine(lineNumber));
    /* istanbul ignore next */
    cov_25t0it8blf().s[233]++;
    return visibleLine.isRenderedRTL();
  }
  visibleRangeForPosition(position) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[31]++;
    const visibleRanges =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[234]++, this._visibleRangesForLineRange(position.lineNumber, position.column, position.column));
    /* istanbul ignore next */
    cov_25t0it8blf().s[235]++;
    if (!visibleRanges) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[50][0]++;
      cov_25t0it8blf().s[236]++;
