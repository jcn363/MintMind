    /* istanbul ignore next */
    (cov_25t0it8blf().b[15][0]++, this._horizontalRevealRequest) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[15][1]++, e.scrollTopChanged)) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[14][0]++;
      const min =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[134]++, Math.min(this._horizontalRevealRequest.startScrollTop, this._horizontalRevealRequest.stopScrollTop));
      const max =
      /* istanbul ignore next */
      (cov_25t0it8blf().s[135]++, Math.max(this._horizontalRevealRequest.startScrollTop, this._horizontalRevealRequest.stopScrollTop));
      /* istanbul ignore next */
      cov_25t0it8blf().s[136]++;
      if (
      /* istanbul ignore next */
      (cov_25t0it8blf().b[17][0]++, e.scrollTop < min) ||
      /* istanbul ignore next */
      (cov_25t0it8blf().b[17][1]++, e.scrollTop > max)) {
        /* istanbul ignore next */
        cov_25t0it8blf().b[16][0]++;
        cov_25t0it8blf().s[137]++;
        // cancel any outstanding horizontal reveal request if someone else scrolls vertically.
        this._horizontalRevealRequest = null;
      } else
      /* istanbul ignore next */
      {
        cov_25t0it8blf().b[16][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[14][1]++;
    }
    cov_25t0it8blf().s[138]++;
    this.domNode.setWidth(e.scrollWidth);
    /* istanbul ignore next */
    cov_25t0it8blf().s[139]++;
    return /* istanbul ignore next */(cov_25t0it8blf().b[18][0]++, this._visibleLines.onScrollChanged(e)) ||
    /* istanbul ignore next */
    (cov_25t0it8blf().b[18][1]++, true);
  }
