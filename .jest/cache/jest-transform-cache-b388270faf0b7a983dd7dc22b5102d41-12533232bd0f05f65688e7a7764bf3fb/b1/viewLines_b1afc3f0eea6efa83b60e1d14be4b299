  onScrollChanged(e) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[20]++;
    cov_25t0it8blf().s[131]++;
    if (
    /* istanbul ignore next */
    (cov_25t0it8blf().b[13][0]++, this._horizontalRevealRequest) &&
    /* istanbul ignore next */
    (cov_25t0it8blf().b[13][1]++, e.scrollLeftChanged)) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[12][0]++;
      cov_25t0it8blf().s[132]++;
      // cancel any outstanding horizontal reveal request if someone else scrolls horizontally.
      this._horizontalRevealRequest = null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[12][1]++;
    }
    cov_25t0it8blf().s[133]++;
