  }
  // ---- end view event handlers
  // ----------- HELPERS FOR OTHERS
  getPositionFromDOMInfo(spanNode, offset) {
    /* istanbul ignore next */
    cov_25t0it8blf().f[24]++;
    const viewLineDomNode =
    /* istanbul ignore next */
    (cov_25t0it8blf().s[144]++, this._getViewLineDomNode(spanNode));
    /* istanbul ignore next */
    cov_25t0it8blf().s[145]++;
    if (viewLineDomNode === null) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[19][0]++;
      cov_25t0it8blf().s[146]++;
      // Couldn't find view line node
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_25t0it8blf().b[19][1]++;
