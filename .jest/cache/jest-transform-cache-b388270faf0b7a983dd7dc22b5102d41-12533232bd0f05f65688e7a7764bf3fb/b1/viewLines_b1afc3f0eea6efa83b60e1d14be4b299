    /* istanbul ignore next */
    (cov_25t0it8blf().b[49][1]++, lineNumber > this._visibleLines.getEndLineNumber())) {
      /* istanbul ignore next */
      cov_25t0it8blf().b[48][0]++;
      cov_25t0it8blf().s[231]++;
