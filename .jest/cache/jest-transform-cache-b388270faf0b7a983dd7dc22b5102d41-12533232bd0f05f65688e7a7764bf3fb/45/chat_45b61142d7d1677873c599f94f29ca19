        /* istanbul ignore next */
        cov_1uhx3nvqot().b[2][0]++;
        cov_1uhx3nvqot().s[7]++;
        existing.group.openEditor(existing.editor);
      } else
      /* istanbul ignore next */
      {
        cov_1uhx3nvqot().b[2][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1uhx3nvqot().b[1][1]++;
    }
  }
}
