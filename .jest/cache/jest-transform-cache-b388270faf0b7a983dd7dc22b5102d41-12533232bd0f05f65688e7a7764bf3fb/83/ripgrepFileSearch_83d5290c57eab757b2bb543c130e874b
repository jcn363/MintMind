  } else {
    /* istanbul ignore next */
    cov_1tmufks6ho().b[4][1]++;
    cov_1tmufks6ho().s[22]++;
    if (folderQuery.disregardParentIgnoreFiles !== false) {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[5][0]++;
      cov_1tmufks6ho().s[23]++;
      args.push('--no-ignore-parent');
    } else
    /* istanbul ignore next */
    {
      cov_1tmufks6ho().b[5][1]++;
    }
  }
  // Follow symlinks
  /* istanbul ignore next */
  cov_1tmufks6ho().s[24]++;
  if (!folderQuery.ignoreSymlinks) {
    /* istanbul ignore next */
    cov_1tmufks6ho().b[6][0]++;
    cov_1tmufks6ho().s[25]++;
    args.push('--follow');
  } else
  /* istanbul ignore next */
  {
    cov_1tmufks6ho().b[6][1]++;
  }
  cov_1tmufks6ho().s[26]++;
  if (config.exists) {
    /* istanbul ignore next */
    cov_1tmufks6ho().b[7][0]++;
    cov_1tmufks6ho().s[27]++;
    args.push('--quiet');
  } else
  /* istanbul ignore next */
  {
    cov_1tmufks6ho().b[7][1]++;
  }
  cov_1tmufks6ho().s[28]++;
  if (numThreads) {
    /* istanbul ignore next */
    cov_1tmufks6ho().b[8][0]++;
    cov_1tmufks6ho().s[29]++;
    args.push('--threads', `${numThreads}`);
  } else
  /* istanbul ignore next */
  {
    cov_1tmufks6ho().b[8][1]++;
  }
  cov_1tmufks6ho().s[30]++;
  args.push('--no-config');
  /* istanbul ignore next */
  cov_1tmufks6ho().s[31]++;
  if (folderQuery.disregardGlobalIgnoreFiles) {
    /* istanbul ignore next */
    cov_1tmufks6ho().b[9][0]++;
    cov_1tmufks6ho().s[32]++;
    args.push('--no-ignore-global');
  } else
  /* istanbul ignore next */
  {
    cov_1tmufks6ho().b[9][1]++;
  }
  cov_1tmufks6ho().s[33]++;
  return {
    args,
    siblingClauses: rgGlobs.siblingClauses
  };
}
function foldersToRgExcludeGlobs(folderQueries, globalExclude, excludesToSkip, absoluteGlobs =
/* istanbul ignore next */
(cov_1tmufks6ho().b[10][0]++, true)) {
  /* istanbul ignore next */
  cov_1tmufks6ho().f[4]++;
  const globArgs =
  /* istanbul ignore next */
  (cov_1tmufks6ho().s[34]++, []);
  let siblingClauses =
  /* istanbul ignore next */
  (cov_1tmufks6ho().s[35]++, {});
  /* istanbul ignore next */
  cov_1tmufks6ho().s[36]++;
  folderQueries.forEach(folderQuery => {
    /* istanbul ignore next */
    cov_1tmufks6ho().f[5]++;
    const totalExcludePattern =
    /* istanbul ignore next */
    (cov_1tmufks6ho().s[37]++, Object.assign({},
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[11][0]++, folderQuery.excludePattern) ||
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[11][1]++, {}),
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[12][0]++, globalExclude) ||
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[12][1]++, {})));
    const result =
    /* istanbul ignore next */
    (cov_1tmufks6ho().s[38]++, globExprsToRgGlobs(totalExcludePattern, absoluteGlobs ?
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[13][0]++, folderQuery.folder.fsPath) :
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[13][1]++, undefined), excludesToSkip));
    /* istanbul ignore next */
    cov_1tmufks6ho().s[39]++;
    globArgs.push(...result.globArgs);
    /* istanbul ignore next */
    cov_1tmufks6ho().s[40]++;
    if (result.siblingClauses) {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[14][0]++;
      cov_1tmufks6ho().s[41]++;
      siblingClauses = Object.assign(siblingClauses, result.siblingClauses);
    } else
    /* istanbul ignore next */
    {
      cov_1tmufks6ho().b[14][1]++;
    }
  });
  /* istanbul ignore next */
  cov_1tmufks6ho().s[42]++;
  return {
    globArgs,
    siblingClauses
  };
}
function foldersToIncludeGlobs(folderQueries, globalInclude, absoluteGlobs =
/* istanbul ignore next */
(cov_1tmufks6ho().b[15][0]++, true)) {
  /* istanbul ignore next */
  cov_1tmufks6ho().f[6]++;
  const globArgs =
  /* istanbul ignore next */
  (cov_1tmufks6ho().s[43]++, []);
  /* istanbul ignore next */
  cov_1tmufks6ho().s[44]++;
  folderQueries.forEach(folderQuery => {
    /* istanbul ignore next */
    cov_1tmufks6ho().f[7]++;
    const totalIncludePattern =
    /* istanbul ignore next */
    (cov_1tmufks6ho().s[45]++, Object.assign({},
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[16][0]++, globalInclude) ||
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[16][1]++, {}),
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[17][0]++, folderQuery.includePattern) ||
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[17][1]++, {})));
    const result =
    /* istanbul ignore next */
    (cov_1tmufks6ho().s[46]++, globExprsToRgGlobs(totalIncludePattern, absoluteGlobs ?
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[18][0]++, folderQuery.folder.fsPath) :
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[18][1]++, undefined)));
    /* istanbul ignore next */
    cov_1tmufks6ho().s[47]++;
    globArgs.push(...result.globArgs);
  });
  /* istanbul ignore next */
  cov_1tmufks6ho().s[48]++;
  return globArgs;
}
function globExprsToRgGlobs(patterns, folder, excludesToSkip) {
  /* istanbul ignore next */
  cov_1tmufks6ho().f[8]++;
  const globArgs =
  /* istanbul ignore next */
  (cov_1tmufks6ho().s[49]++, []);
  const siblingClauses =
  /* istanbul ignore next */
  (cov_1tmufks6ho().s[50]++, {});
  /* istanbul ignore next */
  cov_1tmufks6ho().s[51]++;
  Object.keys(patterns).forEach(key => {
    /* istanbul ignore next */
    cov_1tmufks6ho().f[9]++;
    cov_1tmufks6ho().s[52]++;
    if (
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[20][0]++, excludesToSkip) &&
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[20][1]++, excludesToSkip.has(key))) {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[19][0]++;
      cov_1tmufks6ho().s[53]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1tmufks6ho().b[19][1]++;
    }
    cov_1tmufks6ho().s[54]++;
    if (!key) {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[21][0]++;
      cov_1tmufks6ho().s[55]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1tmufks6ho().b[21][1]++;
    }
    const value =
    /* istanbul ignore next */
    (cov_1tmufks6ho().s[56]++, patterns[key]);
    /* istanbul ignore next */
    cov_1tmufks6ho().s[57]++;
    key = trimTrailingSlash(folder ?
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[22][0]++, getAbsoluteGlob(folder, key)) :
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[22][1]++, key));
    // glob.ts requires forward slashes, but a UNC path still must start with \\
    // #38165 and #38151
    /* istanbul ignore next */
    cov_1tmufks6ho().s[58]++;
    if (key.startsWith('\\\\')) {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[23][0]++;
      cov_1tmufks6ho().s[59]++;
      key = '\\\\' + key.substr(2).replace(/\\/g, '/');
    } else {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[23][1]++;
      cov_1tmufks6ho().s[60]++;
      key = key.replace(/\\/g, '/');
    }
    /* istanbul ignore next */
    cov_1tmufks6ho().s[61]++;
    if (
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[25][0]++, typeof value === 'boolean') &&
    /* istanbul ignore next */
    (cov_1tmufks6ho().b[25][1]++, value)) {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[24][0]++;
      cov_1tmufks6ho().s[62]++;
      if (key.startsWith('\\\\')) {
        /* istanbul ignore next */
        cov_1tmufks6ho().b[26][0]++;
        cov_1tmufks6ho().s[63]++;
        // Absolute globs UNC paths don't work properly, see #58758
        key += '**';
      } else
      /* istanbul ignore next */
      {
        cov_1tmufks6ho().b[26][1]++;
      }
      cov_1tmufks6ho().s[64]++;
      globArgs.push(fixDriveC(key));
    } else {
      /* istanbul ignore next */
      cov_1tmufks6ho().b[24][1]++;
      cov_1tmufks6ho().s[65]++;
      if (
      /* istanbul ignore next */
      (cov_1tmufks6ho().b[28][0]++, value) &&
      /* istanbul ignore next */
      (cov_1tmufks6ho().b[28][1]++, value.when)) {
        /* istanbul ignore next */
        cov_1tmufks6ho().b[27][0]++;
        cov_1tmufks6ho().s[66]++;
        siblingClauses[key] = value;
      } else
      /* istanbul ignore next */
      {
        cov_1tmufks6ho().b[27][1]++;
      }
    }
  });
  /* istanbul ignore next */
  cov_1tmufks6ho().s[67]++;
  return {
    globArgs,
    siblingClauses
  };
}
/**
 * Resolves a glob like "node_modules/**" in "/foo/bar" to "/foo/bar/node_modules/**".
 * Special cases C:/foo paths to write the glob like /foo instead - see https://github.com/BurntSushi/ripgrep/issues/530.
 *
 * Exported for testing
 */
export function getAbsoluteGlob(folder, key) {
  /* istanbul ignore next */
  cov_1tmufks6ho().f[10]++;
  cov_1tmufks6ho().s[68]++;
  return path.isAbsolute(key) ?
  /* istanbul ignore next */
  (cov_1tmufks6ho().b[29][0]++, key) :
  /* istanbul ignore next */
  (cov_1tmufks6ho().b[29][1]++, path.join(folder, key));
}
function trimTrailingSlash(str) {
  /* istanbul ignore next */
  cov_1tmufks6ho().f[11]++;
  cov_1tmufks6ho().s[69]++;
  str = strings.rtrim(str, '\\');
  /* istanbul ignore next */
  cov_1tmufks6ho().s[70]++;
  return strings.rtrim(str, '/');
}
export function fixDriveC(path) {
  /* istanbul ignore next */
  cov_1tmufks6ho().f[12]++;
  const root =
  /* istanbul ignore next */
  (cov_1tmufks6ho().s[71]++, extpath.getRoot(path));
  /* istanbul ignore next */
  cov_1tmufks6ho().s[72]++;
  return root.toLowerCase() === 'c:/' ?
  /* istanbul ignore next */
  (cov_1tmufks6ho().b[30][0]++, path.replace(/^c:[/\\]/i, '/')) :
  /* istanbul ignore next */
  (cov_1tmufks6ho().b[30][1]++, path);
}
