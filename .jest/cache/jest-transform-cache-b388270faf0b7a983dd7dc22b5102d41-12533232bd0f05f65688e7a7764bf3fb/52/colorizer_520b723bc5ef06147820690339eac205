    (cov_gvldeh3tq().s[71]++, lines[i]);
    const tokenizeResult =
    /* istanbul ignore next */
    (cov_gvldeh3tq().s[72]++, tokenizationSupport.tokenizeEncoded(line, true, state));
    /* istanbul ignore next */
    cov_gvldeh3tq().s[73]++;
    LineTokens.convertToEndOffset(tokenizeResult.tokens, line.length);
    const lineTokens =
    /* istanbul ignore next */
    (cov_gvldeh3tq().s[74]++, new LineTokens(tokenizeResult.tokens, line, languageIdCodec));
    const isBasicASCII =
    /* istanbul ignore next */
    (cov_gvldeh3tq().s[75]++, ViewLineRenderingData.isBasicASCII(line, /* check for basic ASCII */true));
    const containsRTL =
    /* istanbul ignore next */
    (cov_gvldeh3tq().s[76]++, ViewLineRenderingData.containsRTL(line, isBasicASCII, /* check for RTL */true));
    const renderResult =
    /* istanbul ignore next */
    (cov_gvldeh3tq().s[77]++, renderViewLine(new RenderLineInput(false, true, line, false, isBasicASCII, containsRTL, 0, lineTokens.inflate(), [], tabSize, 0, 0, 0, 0, -1, 'none', false, false, null, null, 0)));
    /* istanbul ignore next */
    cov_gvldeh3tq().s[78]++;
    html = html.concat(renderResult.html);
    /* istanbul ignore next */
    cov_gvldeh3tq().s[79]++;
    html.push('<br/>');
    /* istanbul ignore next */
    cov_gvldeh3tq().s[80]++;
    state = tokenizeResult.endState;
  }
  /* istanbul ignore next */
  cov_gvldeh3tq().s[81]++;
  return html.join('');
}
