/**
 * Splits incoming pty data into chunks to try prevent data corruption that could occur when pasting
 * large amounts of data.
 */
export function chunkInput(data) {
  /* istanbul ignore next */
  cov_igsf89pkv().f[0]++;
  const chunks =
  /* istanbul ignore next */
  (cov_igsf89pkv().s[0]++, []);
  let nextChunkStartIndex =
  /* istanbul ignore next */
  (cov_igsf89pkv().s[1]++, 0);
  /* istanbul ignore next */
  cov_igsf89pkv().s[2]++;
  for (let i =
  /* istanbul ignore next */
  (cov_igsf89pkv().s[3]++, 0); i < data.length - 1; i++) {
    /* istanbul ignore next */
    cov_igsf89pkv().s[4]++;
    if (
    // If the max chunk size is reached
    /* istanbul ignore next */
    (cov_igsf89pkv().b[1][0]++, i - nextChunkStartIndex + 1 >= 50 /* Constants.WriteMaxChunkSize */) ||
    /* istanbul ignore next */
    (cov_igsf89pkv().b[1][1]++,
    // If the next character is ESC, send the pending data to avoid splitting the escape
    // sequence.
    data[i + 1] === '\x1b')) {
      /* istanbul ignore next */
      cov_igsf89pkv().b[0][0]++;
      cov_igsf89pkv().s[5]++;
      chunks.push(data.substring(nextChunkStartIndex, i + 1));
      /* istanbul ignore next */
      cov_igsf89pkv().s[6]++;
      nextChunkStartIndex = i + 1;
      // Skip the next character as the chunk would be a single character
      /* istanbul ignore next */
      cov_igsf89pkv().s[7]++;
      i++;
    } else
    /* istanbul ignore next */
    {
      cov_igsf89pkv().b[0][1]++;
    }
  }
  // Push final chunk
  /* istanbul ignore next */
  cov_igsf89pkv().s[8]++;
  if (nextChunkStartIndex !== data.length) {
    /* istanbul ignore next */
    cov_igsf89pkv().b[2][0]++;
