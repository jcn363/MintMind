/**
 * Collapses the user's home directory into `~` if it exists within the path, this gives a shorter
 * path that is more suitable within the context of a terminal.
 */
export function collapseTildePath(path, userHome, separator) {
  /* istanbul ignore next */
  cov_1msc6nattq().f[13]++;
  cov_1msc6nattq().s[31]++;
  if (!path) {
    /* istanbul ignore next */
    cov_1msc6nattq().b[5][0]++;
    cov_1msc6nattq().s[32]++;
    return '';
  } else
  /* istanbul ignore next */
  {
    cov_1msc6nattq().b[5][1]++;
  }
  cov_1msc6nattq().s[33]++;
  if (!userHome) {
    /* istanbul ignore next */
    cov_1msc6nattq().b[6][0]++;
    cov_1msc6nattq().s[34]++;
    return path;
  } else
  /* istanbul ignore next */
  {
    cov_1msc6nattq().b[6][1]++;
  }
  // Trim the trailing separator from the end if it exists
  cov_1msc6nattq().s[35]++;
  if (userHome.match(/[\/\\]$/)) {
    /* istanbul ignore next */
    cov_1msc6nattq().b[7][0]++;
    cov_1msc6nattq().s[36]++;
    userHome = userHome.slice(0, userHome.length - 1);
  } else
  /* istanbul ignore next */
  {
    cov_1msc6nattq().b[7][1]++;
  }
  const normalizedPath =
  /* istanbul ignore next */
  (cov_1msc6nattq().s[37]++, path.replace(/\\/g, '/').toLowerCase());
  const normalizedUserHome =
  /* istanbul ignore next */
  (cov_1msc6nattq().s[38]++, userHome.replace(/\\/g, '/').toLowerCase());
  /* istanbul ignore next */
  cov_1msc6nattq().s[39]++;
  if (!normalizedPath.includes(normalizedUserHome)) {
    /* istanbul ignore next */
    cov_1msc6nattq().b[8][0]++;
    cov_1msc6nattq().s[40]++;
    return path;
  } else
  /* istanbul ignore next */
  {
    cov_1msc6nattq().b[8][1]++;
  }
  cov_1msc6nattq().s[41]++;
  return `~${separator}${path.slice(userHome.length + 1)}`;
}
/**
 * Sanitizes a cwd string, removing any wrapping quotes and making the Windows drive letter
 * uppercase.
 * @param cwd The directory to sanitize.
 */
export function sanitizeCwd(cwd) {
  /* istanbul ignore next */
  cov_1msc6nattq().f[14]++;
  cov_1msc6nattq().s[42]++;
  // Sanity check that the cwd is not wrapped in quotes (see #160109)
  if (cwd.match(/^['"].*['"]$/)) {
    /* istanbul ignore next */
    cov_1msc6nattq().b[9][0]++;
    cov_1msc6nattq().s[43]++;
    cwd = cwd.substring(1, cwd.length - 1);
  } else
  /* istanbul ignore next */
  {
    cov_1msc6nattq().b[9][1]++;
  }
  // Make the drive letter uppercase on Windows (see #9448)
  cov_1msc6nattq().s[44]++;
  if (
  /* istanbul ignore next */
  (cov_1msc6nattq().b[11][0]++, OS === 1 /* OperatingSystem.Windows */) &&
  /* istanbul ignore next */
  (cov_1msc6nattq().b[11][1]++, cwd) &&
  /* istanbul ignore next */
  (cov_1msc6nattq().b[11][2]++, cwd[1] === ':')) {
    /* istanbul ignore next */
    cov_1msc6nattq().b[10][0]++;
    cov_1msc6nattq().s[45]++;
    return cwd[0].toUpperCase() + cwd.substring(1);
  } else
  /* istanbul ignore next */
  {
    cov_1msc6nattq().b[10][1]++;
  }
  cov_1msc6nattq().s[46]++;
  return cwd;
}
/**
 * Determines whether the given shell launch config should use the environment variable collection.
 * @param slc The shell launch config to check.
 */
export function shouldUseEnvironmentVariableCollection(slc) {
  /* istanbul ignore next */
  cov_1msc6nattq().f[15]++;
  cov_1msc6nattq().s[47]++;
  return !slc.strictEnv;
}
