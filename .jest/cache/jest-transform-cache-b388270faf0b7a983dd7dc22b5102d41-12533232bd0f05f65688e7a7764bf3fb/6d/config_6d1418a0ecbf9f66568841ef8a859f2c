  {
    cov_6tlm12bg2().b[20][1]++;
  }
  cov_6tlm12bg2().s[63]++;
  if (typeof value === 'string') {
    /* istanbul ignore next */
    cov_6tlm12bg2().b[21][0]++;
    const cleanValue =
    /* istanbul ignore next */
    (cov_6tlm12bg2().s[64]++, value.trim().toLowerCase());
    /* istanbul ignore next */
    cov_6tlm12bg2().s[65]++;
    if (cleanValue === 'true') {
      /* istanbul ignore next */
      cov_6tlm12bg2().b[22][0]++;
      cov_6tlm12bg2().s[66]++;
      return true;
    } else
    /* istanbul ignore next */
