  "scripts": {
    "build": "tauri build",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:tauri-optimized": "node build-alternative.mjs build-all && tauri build",
    "build:tauri-dev": "node build-alternative.mjs build-all --watch",
    "build:cross-platform": "node build-alternative.mjs build-all --platform=${TAURI_PLATFORM:-current}",
    "build:security-audit": "SECURITY_AUDIT=true ANALYZE_BUNDLE=true node build-alternative.mjs build-all",
    "compile": "node build-alternative.mjs build main",
    "compile-build": "NODE_ENV=production node build-alternative.mjs build-all && node --experimental-modules ./scripts/analyze-bundles.js",
    "compile-check-ts-native": "tsgo --project ./src/tsconfig.json --noEmit --skipLibCheck",
    "compile-cli": "node build-alternative.mjs build cli",
    "compile-extensions-build": "node build-alternative.mjs build extensions",
    "compile-tauri": "node build-alternative.mjs build-all && tauri build",
    "compile-tauri-optimized": "NODE_ENV=production SECURITY_AUDIT=true node build-alternative.mjs build-all && tauri build",
    "compile-web": "node build-alternative.mjs build webviews",
    "core-ci": "node ./node_modules/gulp/bin/gulp.js core-ci",
    "core-ci-pr": "node ./node_modules/gulp/bin/gulp.js core-ci-pr",
    "define-class-fields-check": "node build/lib/propertyInitOrderChecker.js && tsgo --project src/tsconfig.defineClassFields.json",
    "dev": "tauri dev",
    "dev:tauri": "npm-run-all -p watch-client tauri:dev",
    "download-builtin-extensions": "node build/lib/builtInExtensions.js",
    "download-builtin-extensions-cg": "node build/lib/builtInExtensionsCG.js",
    "eslint": "node build/eslint",
    "extensions-ci": "node ./node_modules/gulp/bin/gulp.js extensions-ci",
    "extensions-ci-pr": "node ./node_modules/gulp/bin/gulp.js extensions-ci-pr",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "gulp": "node --max-old-space-size=8192 ./node_modules/gulp/bin/gulp.js",
    "hygiene": "node ./node_modules/gulp/bin/gulp.js hygiene",
    "kill-watch-clientd": "deemon --kill bun run watch-client",
    "kill-watch-extensionsd": "deemon --kill bun run watch-extensions",
    "kill-watch-webd": "deemon --kill bun run watch-web",
    "kill-watchd": "deemon --kill bun run watch",
    "lint": "eslint src extensions --ext .ts,.js",
    "minify-vscode": "node ./node_modules/gulp/bin/gulp.js minify-vscode",
    "minify-vscode-reh": "node ./node_modules/gulp/bin/gulp.js minify-vscode-reh",
    "minify-vscode-reh-web": "node ./node_modules/gulp/bin/gulp.js minify-vscode-reh-web",
    "mixin-telemetry-docs": "node build/npm/mixin-telemetry-docs.mjs",
    "monaco-compile-check": "tsgo --project src/tsconfig.monaco.json --noEmit",
    "optimize-loader": "node ./scripts/optimize-loader.js",
    "perf": "node scripts/code-perf.js",
    "playwright-install": "bun exec playwright install",
    "postinstall": "node build/npm/postinstall.js && node scripts/validate-package-managers.js",
    "precommit": "node build/hygiene.js",
    "preinstall": "echo \"Preinstall script disabled\"",
    "restart-watch-webd": "deemon --restart bun run watch-web",
    "restart-watchd": "deemon --restart bun run watch",
    "start": "node --no-warnings=ExperimentalWarning out/main.js",
    "start:esm": "node --no-warnings=ExperimentalWarning --import ./esm-loader.js src/main.ts",
    "stylelint": "node build/stylelint",
    "tauri": "tauri",
    "tauri:build": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri:dev": "tauri dev",
    "tauri:icon": "tauri icon",
    "tauri:info": "tauri info",
    "test": "bunx jest",
    "test:all": "bun run validate:no-electron && bun run test:rust && bun run test:tauri && bun run test:e2e",
    "test:ci": "bunx jest --ci --runInBand --coverage --watchAll=false",
    "test:coverage": "bunx jest --coverage",
    "test:watch": "bunx jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "node scripts/performance-benchmark.js",
    "test:platform": "bash scripts/platform-test-runner.sh",
    "test:rust": "cd src-tauri && cargo test --release",
    "test:tauri": "cargo test",
    "test:tauri:watch": "jest --testPathPattern=test/tauri --watch",
    "tsec-compile-check": "node node_modules/tsec/bin/tsec -p src/tsconfig.tsec.json",
    "update-build-ts-version": "bun install -D typescript@next && bun install -D @typescript/native-preview && (cd build && npm run compile)",
    "update-distro": "node build/npm/update-distro.mjs",
    "update-grammars": "node build/npm/update-all-grammars.mjs",
    "update-localization-extension": "node build/npm/update-localization-extension.js",
    "valid-layers-check": "node build/checker/layersChecker.js && tsgo --project build/checker/tsconfig.browser.json && tsgo --project build/checker/tsconfig.worker.json && tsgo --project build/checker/tsconfig.node.json",
    "validate:no-electron": "bash scripts/validate-no-electron.sh",
    "vscode-dts-compile-check": "tsgo --project src/tsconfig.vscode-dts.json && tsgo --project src/tsconfig.vscode-proposed-dts.json",
    "watch": "node build-alternative.mjs build-all --watch",
    "watch-cli": "node build-alternative.mjs build cli --watch",
    "watch-client": "node build-alternative.mjs build main --watch",
    "watch-clientd": "deemon npm run watch-client",
    "watch-extensions": "node build-alternative.mjs build extensions --watch",
    "watch-extensionsd": "deemon npm run watch-extensions",
    "watch-web": "node build-alternative.mjs build webviews --watch",
    "watch-webd": "deemon npm run watch-web",
    "watchd": "deemon npm run watch",
    "web": "echo 'bun run web' is replaced by './scripts/code-server' or './scripts/code-web'"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@playwright/browser-chromium",
    "@vscode/deviceid",
    "@vscode/policy-watcher",
    "@vscode/spdlog",
